CMD:loadtrash(playerid,params[])
{
	if(PlayerInfo[playerid][pJob] != 3) return SendErrorMessage(playerid, "You're not a garbage man");
    if(!IsPlayerInAnyVehicle(playerid)) return SendErrorMessage(playerid,"You are not inside any vehicles");
    if(!IsPlayerInTrashmaster(playerid)) return SendErrorMessage(playerid,"You are not inside trashmaster");
    if(!IsPlayerInRangeOfPoint(playerid,10.0,2163.0125,-1971.2373,13.5533)) return SendErrorMessage(playerid,"You are not at the LS dumpyard");
    if(GetFactoryResources() <= 999) return SendErrorMessage(playerid,"There are not enough materials inside the factory!");

    new vid = GetPlayerVehicleID(playerid);
    if(VehicleTrash[vid] >= 999) return SendErrorMessage(playerid,"This vehicle is already full");
    VehicleTrash[vid] = 1000;
    UpdateResources(-1000);
    SendErrorMessage(playerid,"You have loaded your truck with 1000 trash. Go to LS dumpyard and type /droptrash");
    return true;
}

CMD:droptrash(playerid,params[])
