	new string[3][256];
	for(new i=0; i < value; i++)
	{
		if(i == 0)
		{
	        format(string[0], 256, "~b~l", string[0]);
	    }
	    else
	    {
	        format(string[0], 256, "%s~b~l", string[0]);
	    }
	}
	new oppfix = opp-value;
	for(new i2=0; i2 < oppfix; i2++)
	{
	    if(i2 == 0)
		{
	        format(string[1], 256, "~w~l", string[1]);
	    }
	    else
	    {
	        format(string[1], 256, "%s~w~l", string[1]);
	    }
	}
	switch(opp)
	{
		case 20: format(string[2], 256, "           %s%s", string[0], string[1]);
