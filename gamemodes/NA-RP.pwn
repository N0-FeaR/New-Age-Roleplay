public OnVehicleMod(playerid, vehicleid, componentid)
{
	new string[128], modok = islegalcarmod(GetVehicleModel(vehicleid), componentid);
	if (!modok)
	{
	    RemoveVehicleComponent(vehicleid, componentid);
        if(GetPVarInt(playerid,"Admin") != 0) return true;
        format(string, sizeof(string), "AdmCmd: %s was banned by Anna-Bot Reason:[Vehicle Mod Hacks].", PlayerName(playerid));
		SendClientMessageToAll(COLOR_LIGHTRED, string);
        //BanExtra(playerid,"Vehicle Mod Hacks","Anna-Bot");
        RemoveVehicleComponent(vehicleid, componentid);
        return 0;
    }
    if(IsNotAEngineCar(vehicleid) || IsHelmetVehicle(vehicleid) || VehicleInfo[vehicleid][vType] != 2)
    {
		if(GetPVarInt(playerid,"Admin") != 0) return true;
		RemoveVehicleComponent(vehicleid, componentid);
		format(string, sizeof(string), "AdmCmd: %s was banned by Anna-Bot Reason:[Vehicle Mod Hacks].", PlayerName(playerid));
		SendClientMessageToAll(COLOR_LIGHTRED, string);
	    //BanExtra(playerid,"Vehicle Mod Hacks","Anna-Bot");
        RemoveVehicleComponent(vehicleid, componentid);
	}
    else
    {
        if(strcmp(CarInfo[VehicleInfo[vehicleid][vOwned]][cOwner], PlayerName(playerid), true) == 0)
		{
		    return true; // End code.
		}
        else
        {
            if(GetPVarInt(playerid,"Admin") != 0) return true;
            RemoveVehicleComponent(vehicleid, componentid);
            format(string, sizeof(string), "AdmCmd: %s was banned by Anna-Bot Reason:[Vehicle Mod Hacks].", PlayerName(playerid));
		    SendClientMessageToAll(COLOR_LIGHTRED, string);
            //BanExtra(playerid,"Vehicle Mod Hacks","Anna-Bot");
            RemoveVehicleComponent(vehicleid, componentid);
        }
    }
    CallRemoteFunction("VMC_OnVehicleMod","iii", playerid, vehicleid, componentid);
    return true;
