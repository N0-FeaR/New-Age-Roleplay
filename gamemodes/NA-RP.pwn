    	}
	    case 22,23,24,26,28,32:
	    {
     		switch(GetPlayerSkin(playerid))
       		{
	        	case 265 .. 267, 280 .. 287: SetPlayerAttachedObject(playerid, HOLDBJECT_GUN2, GetGunObjectID(GetPVarInt(playerid, "InvWeapon")), 8, -0.044177, 0.000000, 0.092454, 246.994583, 0.000000, 0.000000, 1.000000, 1.000000, 1.000000);
