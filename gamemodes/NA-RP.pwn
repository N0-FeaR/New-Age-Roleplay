    format(string, sizeof(string),"%s placed into your inventory !",gunname);
    SendClientMessage(playerid, COLOR_WHITE, string);
    RemovePlayerWeaponEx(playerid, GetPlayerWeapon(playerid));
    SetPVarInt(playerid, "Delay", GetCount()+3000);
    switch(PlayerInfo[playerid][pInvWeapon])
	{
	    case 25: if(PlayerInfo[playerid][pFaction] == 1 || PlayerInfo[playerid][pFaction] == 7) SetPlayerAttachedObject( playerid, HOLDBJECT_GUN2, GetGunObjectID(PlayerInfo[playerid][pInvWeapon]), 1, -0.157276, -0.124338, -0.098805, 195.769546, 13.859096, 0.000000, 1.000000, 1.000000, 1.000000);
		case 27,29,30,31,33,34:
		{
		    if(PlayerInfo[playerid][pFaction] == 1 || PlayerInfo[playerid][pFaction] == 7) SetPlayerAttachedObject( playerid, HOLDBJECT_GUN2, GetGunObjectID(PlayerInfo[playerid][pInvWeapon]), 1, 0.110000, 0.180000, -0.265000, 180.769546, 142.859096, 0.000000, 1.000000, 1.000000, 1.000000);
			else SetPlayerAttachedObject(playerid, HOLDBJECT_GUN2, GetGunObjectID(PlayerInfo[playerid][pInvWeapon]), 1, 0.139415, -0.167970, 0.120848, 0.000000, 152.342666, 0.000000);
		}
		case 22,23,24,26,28,32:
		{
		    switch(GetPlayerSkin(playerid))
		    {
			    case 265 .. 267, 280 .. 287: SetPlayerAttachedObject(playerid, HOLDBJECT_GUN2, GetGunObjectID(PlayerInfo[playerid][pInvWeapon]), 8, -0.044177, 0.000000, 0.092454, 246.994583, 0.000000, 0.000000, 1.000000, 1.000000, 1.000000);
			}
		}
	}
	return 1;
}
