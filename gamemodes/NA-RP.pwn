CMD:accept(playerid, params[])
{
	new string[128], sendername[MAX_PLAYER_NAME], giveplayer[MAX_PLAYER_NAME], Float:x, Float:y, Float:z;
	if(PlayerLogged[playerid] == false) SendErrorMessage(playerid, "You're not logged in.");
    if(IsPlayerConnected(playerid))
	{
        if(isnull(params))
		{
            SendSyntaxMessage(playerid, "/accept [name]");
            SendSyntaxMessage(playerid, "Available names: Invite,");
            return 1;
        }
        if(strcmp(params, "death", true) == 0)
   	    {
   	        if(PlayerInfo[playerid][pDead] != 2) return SendClientMessage(playerid, COLOR_WHITE, "You have not even died yet.");
   	        if(GetPVarInt(playerid, "DeathTime") > GetCount()) return SendClientMessage(playerid, COLOR_WHITE, "You must wait before accepting death.");
   	        if(GetPVarInt(playerid, "JailTime") >= 1) return SendClientMessage(playerid, COLOR_WHITE, "You cannot accept death inside the prison, you must wait for a gaurd or prisoner to RP with you!");
   	        format(sendername, sizeof(sendername), "%s", PlayerNameEx(playerid));
   	        format(string, sizeof(string), "%s has accepted death.",sendername);
   	        SendNearbyMessage(playerid, 30.0, COLOR_WHITE, string);
   	        DeletePVar(playerid,"DeathTime");
   	        PlayerInfo[playerid][pCuffed] = 0;
  	   		if(!SetPlayerAttachedObject(playerid, 0, 19418, 6, -0.011000, 0.028000, -0.022000, -15.600012, -33.699977,-81.700035, 0.891999, 1.000000, 1.168000))
   	        SetPVarInt(playerid, "InvWeapon", 0); SetPVarInt(playerid, "InvAmmo", 0);
            ResetPlayerWeaponsEx(playerid);
            SetPVarInt(playerid, "FadeToDeath", 1);
           	FadeTime[playerid]=0;
	        FadeScreen(playerid, 1);
	        //ClearChatbox(playerid,50);
   	        //DeletePVar(playerid,"Blinded"), TextDrawHideForPlayer(playerid,BlindDraw);
   	    }
        if(strcmp(params, "invite", true) == 0)
		{
			if(factionInvited[playerid] == false) return SendErrorMessage(playerid, "You're not invited to any faction.");

			new faction = PlayerInfo[Player_Invited[playerid]][pFaction];

		 	PlayerInfo[playerid][pFaction] = PlayerInfo[Player_Invited[playerid]][pFaction];
		 	PlayerInfo[playerid][fRank] = 1;
			FactionInfo[faction][fMembers] += 1;

			format(string, sizeof(string), "[Faction:] %s has joined the faction.", PlayerName(playerid));
			SendFactionMessage(PlayerInfo[playerid][pFaction], COLOR_FCHAT, string);
		}
		else if(strcmp(params, "mech", true) == 0)
   	    {
   	        if(PlayerInfo[playerid][pDead] > 0) return SendErrorMessage(playerid, "Cannot use this command while dead.");
	        if(PlayerInfo[playerid][pMute] == 1) return SendErrorMessage(playerid, "You are currently muted.");
   	        if(PlayerInfo[playerid][pJob] != 4) return SendErrorMessage(playerid, "You are not a Mechanic.");
   	        if(GetPlayerState(playerid) != PLAYER_STATE_DRIVER) return SendClientMessage(playerid, COLOR_GREY, "You must be in a vehicle driver to use this.");
   	        if (MechCall == 501) return SendClientMessage(playerid, COLOR_WHITE, "No mechanic calls available.");
   	        format(sendername, sizeof(sendername), "%s", PlayerName(playerid));
	    	format(giveplayer, sizeof(giveplayer), "%s", PlayerName(MechCall));
			format(string, sizeof(string), "~h~~g~You have accepted %s's call !", giveplayer);
			SendClientMessage(playerid, COLOR_WHITE, string);
			SendClientMessage(MechCall, COLOR_YELLOW, "TXT: Your requested towtruck will arrive shortly, Sender: 'LS Mechanic Co.'.");
			PlayerPlaySound(MechCall, 1052, 0.0, 0.0, 0.0);
			PlayAudioStreamForPlayer(MechCall,"http://fresh.pNA-RP.net/gta/cellphone/vibrate.mp3");
            GetPlayerPos(MechCall,x,y,z);
            SetPlayerCheckpoint(playerid,x,y,z,5.0);
            SetPVarInt(playerid, "Checkpoint", CHECKPOINT_NONE);
            GameTextForPlayer(playerid, "~w~Mechanic Caller~n~~r~Goto redmarker", 5000, 1);
		    MechCall = 501;
   	    }
		else if(strcmp(params, "call", true) == 0)
   	    {
   	        format(sendername, sizeof(sendername), "%s", PlayerName(playerid));
			switch(PlayerInfo[playerid][pFaction])
			{
			    case 2:
			    {
					if(EMTCall == 501) return SendErrorMessage(playerid, "No-one called the EMT service.");
					if(IsPlayerConnected(EMTCall))
					{
					    format(giveplayer, sizeof(giveplayer), "%s", PlayerName(EMTCall));
					    format(string, sizeof(string), "DISPATCH: %s has traced the call of %s.", sendername, giveplayer);
				        SendFactionMessage(2, COLOR_BLUE, string);
				        GetPlayerPos(EMTCall,x,y,z);
            	        AcceptCallCP = CreateDynamicCP(x,y,z,5, -1, -1, playerid, 5000.0);
            	        //SetPVarInt(playerid, "Checkpoint", CHECKPOINT_NONE);
					    EMTCall=501;
					}
					else EMTCall=501, SendErrorMessage(playerid, "Player is no longer connected.");
			    }
			    case 1:
			    {
			        if(PoliceCall == 501) return SendErrorMessage(playerid, "No-one called the Police service.");
					if(IsPlayerConnected(PoliceCall))
					{
					    format(giveplayer, sizeof(giveplayer), "%s", PlayerName(PoliceCall));
					    format(string, sizeof(string), "DISPATCH: %s has traced the call of %s.", sendername, giveplayer);
				        SendFactionMessage(1, COLOR_BLUE, string);
				        GetPlayerPos(PoliceCall,x,y,z);
            	        SetPlayerCheckpoint(playerid,x,y,z,5.0);
            	        //SetPVarInt(playerid, "Checkpoint", CHECKPOINT_NONE);
					    PoliceCall=501;
					}
					else PoliceCall=501, SendErrorMessage(playerid, "Player is no longer connected.");
			    }
			    default: SendErrorMessage(playerid, "You do not have access to this command!");
			}
		}
	}
	else SendErrorMessage(playerid, "That player is not connected!");
	return 1;
}
CMD:house(playerid, params[])
{
	new choose[128],amount,amount2,gunname[128], id = IsPlayerInsideHouse(playerid);
	if(PlayerLogged[playerid] == false) SendErrorMessage(playerid, "You're not logged in.");
	if(sscanf(params, "s[128]I(-1)I(-1)", choose, amount, amount2))
	{
 	    SendSyntaxMessage(playerid, "/house {FFFFFF}[balance/deposit/withdraw/rentable/setrent/rent/putgun/takegun/lock]");
 	    SendSyntaxMessage(playerid, "/house {FFFFFF}[putweed/putcocaine/putmeth/putecstasy/putcrack/takeweed/takecocaine/takeecstasy/putcrack]");
	}
	else
	{
		if(strcmp(choose, "balance", true) == 0)
		{
			if(id != PlayerInfo[playerid][HouseID]) return SendErrorMessage(playerid, "You need to be inside your house to use this.");
	 		if(PlayerInfo[playerid][HouseID] == 0) return SendErrorMessage(playerid, "You do not own a house!");
			SendServerMessage(playerid, "You have $%d in your house vault.", HouseInfo[PlayerInfo[playerid][HouseID]][hMoney]);
		}
	    else if(strcmp(choose, "deposit", true) == 0)
		{
			if(id != PlayerInfo[playerid][HouseID]) return SendErrorMessage(playerid, "You need to be inside your house to use this.");
		 	if(PlayerInfo[playerid][HouseID] == 0) return SendErrorMessage(playerid, "You do not own a house!");
			if(amount == (-1)) return SendClientMessage(playerid, COLOR_GREEN, "USAGE: /house {FFFFFF}[deposit] [amount]");
		    if(amount < 1 || amount > 10000000) return SendClientMessage(playerid, COLOR_GREY, "Cannot go under 1 or above 10000000.");
		 	if (amount > GetPlayerMoneyEx(playerid)) return SendErrorMessage(playerid, "You do not have that much money.");

			GivePlayerMoneyEx(playerid, -amount);
			HouseInfo[id][hMoney] += amount;
			SendServerMessage(playerid, "You have successfully deposited $%d to your house vault.", amount);
		}
	    else if(strcmp(choose, "withdraw", true) == 0)
		{
			if(id != PlayerInfo[playerid][HouseID]) return SendErrorMessage(playerid, "You need to be inside your house to use this.");
		 	if(PlayerInfo[playerid][HouseID] == 0) return SendErrorMessage(playerid, "You do not own a house!");
			if(amount == (-1)) return SendClientMessage(playerid, COLOR_GREEN, "USAGE: /house {FFFFFF}[withdraw] [amount]");
		    if(amount < 1 || amount > 10000000) return SendClientMessage(playerid, COLOR_GREY, "Cannot go under 1 or above 10000000.");
		 	if (amount > HouseInfo[id][hMoney]) return SendErrorMessage(playerid, "You do not have that much money in your house vault.");

			GivePlayerMoneyEx(playerid, amount);
			HouseInfo[id][hMoney] -= amount;
