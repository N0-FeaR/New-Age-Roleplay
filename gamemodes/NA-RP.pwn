//----------------------------------------------------------
forward Float:GetDistanceBetweenPoints(Float:x1, Float:y1, Float:z1, Float:x2, Float:y2, Float:z2);
stock Float:GetDistanceBetweenPoints(Float:x1, Float:y1, Float:z1, Float:x2, Float:y2, Float:z2)
{
    x1 -= x2;
    y1 -= y2;
    z1 -= z2;
    return floatsqroot((x1 * x1) + (y1 * y1) + (z1 * z1));
}
//----------------------------------------------------------
stock CheckPassengers(vehicleid)
{
        for(new i = 0; i < MAX_PLAYERS; i++)
        {
                if(IsPlayerInAnyVehicle(i))
                {
                        if(GetPlayerVehicleID(i) == vehicleid && i != GetVehicleDriver(vehicleid))
                        {

                                return 1;

                        }
                }
        }
        return 0;
}
//----------------------------------------------------------
stock GetNumOfPassengers(driver)
{
	new count = 0;
    for(new i = 0; i < MAX_PLAYERS; i++)
    {
    	if(IsPlayerInAnyVehicle(i))
     	{
      		if(GetPlayerVehicleID(i) == GetPlayerVehicleID(driver) && i != driver)
        	{
         		count++;
       		}
       	}
	}
 	return count;
}
//----------------------------------------------------------
stock GetFirstName(playerid)
{
    new ns[2][MAX_PLAYER_NAME], n[MAX_PLAYER_NAME];

    GetPlayerName(playerid,n,MAX_PLAYER_NAME);
    split(n, ns, '_');
    return ns[0];
}
//----------------------------------------------------------
stock GetPlayerNameEx(playerid)
{
        new pname[MAX_PLAYER_NAME];
        GetPlayerName(playerid,pname,sizeof(pname));
        return pname;
}
//----------------------------------------------------------
stock IsAtGasStation(playerid)
{
    for(new h = 0; h < sizeof(GasStations); h++)
	{
	    if(IsPlayerInRangeOfPoint(playerid,7.0,GasStations[h][0], GasStations[h][1], GasStations[h][2])) return true;
	}
	return false;
}
//----------------------------------------------------------
stock FillGas(playerid,amount)
{
	new string[128],idcar = GetPlayerVehicleID(playerid),price;
    TogglePlayerControllable(playerid,true);
    if(GetPlayerState(playerid) != PLAYER_STATE_DRIVER) return SendErrorMessage(playerid,"You are not even inside a vehicle !");
    new amountcheck = amountcheck = amount + VehicleInfo[idcar][vFuel];
    if(FreeCustomer(playerid)) price = 0;
    else price = amount*GasPrice;
	if(amountcheck < 100)
	{
		if(GetPlayerMoneyEx(playerid) >= price)
		{
		    PlayerPlaySound(playerid,1054, 0.0, 0.0, 0.0);
		    format(string, sizeof(string),"~r~-$%d",price);
		    GameTextForPlayer(playerid, string, 5000, 1);
		    GivePlayerMoneyEx(playerid,-price);
		    VehicleInfo[idcar][vFuel] += amount;
		    amountcheck = amount;
		    format(string, sizeof(string), "Your vehicle has been filled up with %d liters of fuel for $%d.", amount, price);
		    SendClientMessage(playerid, COLOR_WHITE, string);
	    }
	    else SendEnough(playerid,"dollars", price, GetPlayerMoneyEx(playerid));
	}
	else SendErrorMessage(playerid,"You are not able to pump your vehicle with this amount of liters.");
    return 0;
}
//----------------------------------------------------------
stock FreeCustomer(playerid)
{
    if(PlayerInfo[playerid][pDonateRank] > 0 || PlayerInfo[playerid][pFaction] == 1
	|| PlayerInfo[playerid][pFaction] == 2) return true;
	return false;
}
//----------------------------------------------------------
stock PrintSpeedo(playerid, Float:speed)
{
	new ftext[50], stext[50], sspeed = floatround(speed);
	switch(sspeed)
	{
	    case 100 .. 5000: format(stext, 50, "  ~w~%d", sspeed);
	    case 10 .. 99: format(stext, 50, "   ~w~%d", sspeed);
	    case 0 .. 9: format(stext, 50, "    ~w~%d", sspeed);
	}
	TextDrawSetString(Text:UsedDraw[playerid], stext);
	switch(VehicleInfo[GetPlayerVehicleID(playerid)][vFuel])
	{
	    case 100 .. 200: ftext="~w~IIIIIIIIII";
	    case 90 .. 99: ftext="~w~IIIIIIIII~r~I";
	    case 80 .. 89: ftext="~w~IIIIIIII~r~II";
	    case 70 .. 79: ftext="~w~IIIIIII~r~III";
	    case 60 .. 69: ftext="~w~IIIIII~r~IIII";
	    case 50 .. 59: ftext="~w~IIIII~r~IIIII";
	    case 40 .. 49: ftext="~w~III~r~IIIIIII";
	    case 30 .. 39: ftext="~w~II~r~IIIIIIII";
	    case 10 .. 29: ftext="~w~I~r~IIIIIIIII";
	    case 0 .. 9: ftext="~r~IIIIIIIIII";
	}
	TextDrawSetString(Text:VehicleIDraw[playerid], ftext);
	return true;
}
