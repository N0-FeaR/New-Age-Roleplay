public OnPlayerEnterCheckpoint(playerid)
{
	switch(GetPVarInt(playerid, "Checkpoint"))
	{
		case CHECKPOINT_NONE: DisablePlayerCheckpoint(playerid);
        case CHECKPOINT_STORE:
        {
            if(GetPVarInt(playerid, "OCC") == 0)
			{
			    //if(BizzProductC(GetPVarInt(playerid, "BizzEnter"))) return SendClientMessage(playerid,COLOR_GREY,"There is no products available in this business !");
			    new rand = random(5)+1; // AI
				switch(rand)
				{
				    case 1: SendClientMessage(playerid,COLOR_FADE,"24/7 Cashier Says: Welcome, what can I get for you today?");
				    case 2: SendClientMessage(playerid,COLOR_FADE,"24/7 Cashier Says: Hey, how are you?");
				    case 3: SendClientMessage(playerid,COLOR_FADE,"24/7 Cashier Says: Welcome.");
				    case 4: SendClientMessage(playerid,COLOR_FADE,"24/7 Cashier Says: Hello there, what can I do for you today?");
				    default: SendClientMessage(playerid,COLOR_FADE,"24/7 Cashier Says: There's a variety of items to purchase.");
				}
				ShowStoreDialog(playerid);
			    SetPVarInt(playerid, "OCC", 1);
	        }
        }
	}
	if (IsPlayerInRangeOfPoint(playerid, 7.0, 2362.9297,-1643.3599,14.3082)) //Checkpoint 1
    {
        GivePlayerMoneyEx(playerid, 80);
        DisablePlayerCheckpoint(playerid);
        SendServerMessage(playerid, "You have delivered the pizza before it went cold, go and get more and you earned 80$!");
        DeletePVar(playerid, "DeliveringP");
    }
    else if (IsPlayerInRangeOfPoint(playerid, 7.0,2523.2732,-1679.4453,15.4970)) //Checkpoint 2
    {
        GivePlayerMoneyEx(playerid, 120);
        DisablePlayerCheckpoint(playerid);
        SendServerMessage(playerid, "You have delivered the pizza before it went cold, go and get more and you earned 120$!");
        DeletePVar(playerid, "DeliveringP");
    }
    else if (IsPlayerInRangeOfPoint(playerid, 7.0, 2178.5088,-1660.3746,14.9678)) //Checkpoint 3
    {
        GivePlayerMoneyEx(playerid, 150);
        DisablePlayerCheckpoint(playerid);
        SendServerMessage(playerid, "You have delivered the pizza before it went cold, go and get more and you earned 150$!");
        DeletePVar(playerid, "DeliveringP");
    }
	return 1;
}
