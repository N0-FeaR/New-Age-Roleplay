	{
		if(PlayerLogged[playerid] == true)
		{
		    if(PlayerInfo[playerid][pPayDay] >= 60)
		    {
		        account = PlayerInfo[playerid][pBank];
		        if(account >= 0 && account <= 500000) Tax = TaxValue;
		        if(account >= 500000 && account <= 800000) Tax = TaxValue*1;
		        if(account >= 800000 && account <= 1000000) Tax = TaxValue*2;
		        if(account >= 1000000 && account <= 3000000) Tax = TaxValue*3;
		        if(account >= 3000000 && account <= 5000000) Tax = TaxValue*4;
		        if(account >= 5000000 && account <= 7000000) Tax = TaxValue*5;
		        if(account >= 7000000 && account <= 10000000) Tax = TaxValue*6;
		        if(account >= 10000000) Tax = TaxValue*20;
		        total +=PlayerInfo[playerid][pPayCheck] / 5;
