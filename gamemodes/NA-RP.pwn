CMD:admins(playerid, params[])
{
    new sendername[MAX_PLAYER_NAME],string[128],admtext[128];
    new found = 0;
    foreach (Player,i)
    {
        if(PlayerInfo[i][pAdmin] >= 1)
		{
		    found++;
		}
	}
	if(found == 0) return SendClientMessage(playerid, COLOR_FADE, "There is no admins online.");
	SendClientMessage(playerid, COLOR_WHITE, "__________________________________________________");
    SendClientMessage(playerid, COLOR_WHITE, "                           Admins Online:");
    foreach (Player,i)
    {
        if(PlayerInfo[i][pAdmin] >= 1)
        {
            format(sendername, sizeof(sendername), "%s", PlayerName(i));
      		switch(PlayerInfo[i][pAdmin])
            {
                case 5: admtext = "Lead Administrator";
                case 4: admtext = "Senior Administrator";
                case 3: admtext = "General Administrator";
                case 2: admtext = "Junior Administrator";
                case 1: admtext = "Server Moderator";
                default: admtext = "undefined level administrator";
            }
			switch(PlayerInfo[i][pAdminDuty])
			{
			    case 0:
			    {
			        if(strcmp(PlayerInfo[i][pAdminName], "None", true) == 0)
			        {
		     	        format(string, sizeof(string), "%s: %s [ID: %d]", admtext, sendername, i);
			            SendClientMessage(playerid, COLOR_FADE, string);
					}
					else
					{
					    format(string, sizeof(string), "%s: (%s): %s [ID: %d]", admtext, PlayerInfo[i][pAdminName], sendername, i);
			            SendClientMessage(playerid, COLOR_FADE, string);
					}
			    }
			    case 1:
			    {
			        if(strcmp(PlayerInfo[i][pAdminName], "None", true) == 0)
			        {
		     	        format(string, sizeof(string), "%s: %s [ID: %d] (On Duty)", admtext, sendername, i);
			            SendClientMessage(playerid, 0x40808096, string);
					}
					else
					{
					    format(string, sizeof(string), "%s: (%s): %s [ID: %d] (On Duty)", admtext, PlayerInfo[i][pAdminName], sendername, i);
			            SendClientMessage(playerid, 0x40808096, string);
					}
			    }
			}
        }
    }
    SendClientMessage(playerid, COLOR_WHITE, "__________________________________________________");
	return 1;
}
CMD:help(playerid, params[])
