	new arrCoords[5][64], strFromFile2[256];
	new File: file = fopen("buyablecars/normalcars.ini", io_read);
	if (file)
	{
		new idx;
		while (idx < sizeof(NormalCar))
		{
			fread(file, strFromFile2);
			split(strFromFile2, arrCoords, ',');
			strmid(NormalCar[idx][carname], arrCoords[0], 0, strlen(arrCoords[0]), 255);
			NormalCar[idx][carprice] = strval(arrCoords[1]);
			NormalCar[idx][carlevel] = strval(arrCoords[2]);
			NormalCar[idx][carID] = strval(arrCoords[3]);
			NormalCar[idx][carQ] = strval(arrCoords[4]);
			idx++;
		}
	}
	fclose(file);
	return 1;
}
