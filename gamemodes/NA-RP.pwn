CMD:collecttrash(playerid,params[])
{
	if(PlayerInfo[playerid][pJob] != 3) return SendErrorMessage(playerid, "You're not a garbage man");
    if(IsPlayerInAnyVehicle(playerid)) return SendErrorMessage(playerid,"You cannot do this while inside vehicle");
    new Float:x, Float:y, Float:z;
    for(new i = 0; i < sizeof(TrashcanObject); i++)
	{
        GetDynamicObjectPos(TrashcanObject[i],x,y,z);
        if(IsPlayerInRangeOfPoint(playerid, 10.0, x,y,z))
		{
            if(TrashcanResources[i] <= 0) return SendErrorMessage(playerid,"This trash can is empty");
            if(GetPlayerTrash(playerid) >= 200) return SendErrorMessage(playerid,"You already have too much trash on you");
            new string[64];
            TotalTrashCollected[playerid] += TrashcanResources[i];
            format(string,sizeof(string),"You took %i trash from this can",TrashcanResources[i]);
            SendClientMessage(playerid,COLOR_LIGHTRED,string);
            GivePlayerTrash(playerid,TrashcanResources[i]);
            UpdateTrashcanResources(i, 0);
            UpdatePlayerTrashSkill(playerid);
            printf("Player trash skill %i",GetPlayerTrashSkill(playerid));
            break;
        }
    }
    return true;
}

CMD:leavetrash(playerid,params[])
{
	if(PlayerInfo[playerid][pJob] != 3) return SendErrorMessage(playerid, "You're not a garbage man");
    if(TrashOnPlayer[playerid] <= 0) return SendErrorMessage(playerid,"You dont have any trash on!");
    if(!IsPlayerInRangeOfPoint(playerid,10.0,2194.0925, -1973.0626, 13.3654)) return SendErrorMessage(playerid,"You are not near the dumpyard in LS");
    if(GetFactoryResources() >= MAX_FACTORY_RESOURCES) return SendErrorMessage(playerid,"Factory is already full!");
    PayByResources(playerid,TrashOnPlayer[playerid]);
    GivePlayerTrash(playerid,-TrashOnPlayer[playerid]);
    UpdateResources(TrashOnPlayer[playerid]);
    GivePlayerMoneyEx(playerid,-GetTrashTax(playerid));
    new string[128];
    format(string,sizeof(string),"You have paid %i$ as a state tax",GetTrashTax(playerid));
    SendClientMessage(playerid,COLOR_LIGHTRED,string);
    return true;
}


