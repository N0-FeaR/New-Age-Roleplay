CMD:tapwater(playerid, params[])
{
    new Float:health;
    if(PlayerLogged[playerid] == false) SendErrorMessage(playerid, "You're not logged in.");
    if(PlayerInfo[playerid][pCuffed] > 0) return SendClientMessage(playerid, COLOR_GREY, "You can't do this while handcuffed.");
    if(PlayerInfo[playerid][pDead] > 0) return SendClientMessage(playerid, COLOR_WHITE, "Cannot use this command while dead.");
    GetPlayerHealth(playerid,health);
	for(new h = 0; h < sizeof(HouseInfo); h++)
	{
	    if(IsPlayerInRangeOfPoint(playerid,20.0,HouseInfo[h][hExitX],HouseInfo[h][hExitY],HouseInfo[h][hExitZ]))
	    {
			if(GetPlayerVirtualWorld(playerid) == h+5000)
			{
				if(GetPlayerMoneyEx(playerid) >= 10)
				{
    				if(health >= 99) return SendClientMessage(playerid,COLOR_GREY,"Health is too full.");
				    SendNearbyMessage(playerid, 30.0, COLOR_PURPLE, "*** %s drinks from the tap water.", PlayerName(playerid));
				    GivePlayerMoneyEx(playerid,-10);
				    SetPlayerHealthEx(playerid,health+40.0);
				    GameTextForPlayer(playerid, "~r~-$10", 5000, 1);
				    ApplyAnimation(playerid, "GANGS", "drnkbr_prtl", 3.0, 0, 0, 0, 0, 0);
				 	HouseInfo[h][hUsage]++;
				}
				else SendEnough(playerid,"dollars", 10, GetPlayerMoneyEx(playerid));
	        }
	    }
	}
	return 1;
}
CMD:lights(playerid, params[])
{
    if(PlayerLogged[playerid] == false) SendErrorMessage(playerid, "You're not logged in.");
