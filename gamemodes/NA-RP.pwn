stock GivePlayerWeaponEx(playerid, weaponid, ammos)
{
    SetPVarInt(playerid, "GunWarning", 0);
    //PlayerInfo[playerid][pGunWarnings][weaponid]=0;
    if(GetPVarInt(playerid, "Member") != 7)
    {
        if(weaponid >= 22 && weaponid <= 34)
        {
            if(ammos > WeaponAmmos[weaponid][0])
            {
                RemovePlayerWeaponEx(playerid, weaponid);
                ammos = WeaponAmmos[weaponid][0];
            }
        }
	}
	PlayerWeapons[playerid][weaponid] = 1;
	if(PlayerAmmo[playerid][weaponid] <= 0) PlayerAmmo[playerid][weaponid] = ammos;
	else if(PlayerAmmo[playerid][weaponid] >= 1) PlayerAmmo[playerid][weaponid] += ammos;
	return GivePlayerWeapon(playerid, weaponid, ammos);
}
