CMD:gmx(playerid, params[])
{
	new id;
	if(PlayerLogged[playerid] == false) SendErrorMessage(playerid, "You're not logged in.");
	if(GMX[playerid] == 1) return SendErrorMessage(playerid, "You cannot use this right now!");
	if(PlayerInfo[playerid][pAdmin] < 4) return SendErrorMessage(playerid, "You are not authorized to use this command.");
 	foreach (new i : Player)
	{
		OnPlayerDataSave(i);
		TogglePlayerControllable(i,0);
		PlayerTextDrawShow(i,Countdown[i]);
		PlayerTime[i] = 20;
	}
	GMX[playerid] += 1;
	SaveDroppedGuns();
	SaveHouse(id);
	SaveBusiness(id);
	SaveDoor(id);
	SaveCars();
	SaveCars2();
	PlayerTextDrawShow(playerid,Countdown[playerid]);
	PlayerTime[playerid] = 20;
	PlayerTimer[playerid] = SetTimerEx("Counter",1000,true,"i",playerid);
	SetTimerEx("RestartServer", 20000, 0, "i", playerid);
	SetPlayerSkin(playerid, PlayerInfo[playerid][pSkin]);
	SendClientMessageToAll(COLOR_LIGHTRED, "[SERVER:] The server will restart in 20 seconds.");
	SendClientMessageToAll(COLOR_LIGHTRED, "[SERVER:] All players will be freeze due saving all accounts info.");
	return 1;
}
