    if(GetPVarInt(playerid, "Dead") != 0) return SendClientMessage(playerid, COLOR_WHITE, "You must be not dead to use this.");
    if(PlayerInfo[playerid][pInvWeapon] != 0) return SendClientMessage(playerid, COLOR_GREY, "You already have a weapon in your inventory.");
    if(PlayerWeapons[playerid][GetPlayerWeapon(playerid)] == 0) return SendClientMessage(playerid, COLOR_LIGHTRED, "The weapon you currently have in your hand is labeld a Hacked Weapon, You are either SA-MP Bugged or Cheating !");
    if(GetPlayerWeapon(playerid) == 0) return SendClientMessage(playerid, COLOR_GREY, "You don't have a weapon equiped.");
    if(GetPVarInt(playerid, "Delay") > GetCount()) return SendClientMessage(playerid, COLOR_LIGHTRED, "You must wait before performing this command !");
    if(GetPlayerPing(playerid) >= 500) return SendClientMessage(playerid, COLOR_WHITE, "Your ping is too high to do this command !");
    if(Tazer[playerid] != 0) return SendClientMessage(playerid, COLOR_WHITE, "You are not able to use this !");
    new sweapon, sammo, ammo;
   	for (new i=0; i<9; i++)
	{
	    GetPlayerWeaponData(playerid, i, sweapon, sammo);
