    if(PlayerLogged[playerid] == false) SendErrorMessage(playerid, "You're not logged in.");
    if(PlayerInfo[playerid][pJob] == 0) return SendClientMessage(playerid, COLOR_WHITE, "You don't have a job to quit from.");
    if(GetPVarInt(playerid, "JobTime") > 0)
    {
		new string[128];
		format(string, sizeof(string),"You must wait %d minutes before quiting your job.",GetPVarInt(playerid, "JobTime"));
		return SendClientMessage(playerid, COLOR_WHITE, string);
    }
	SendClientMessage(playerid, COLOR_WHITE, "You are now unemployed !");
	PlayerInfo[playerid][pJob] = 0;
	//SetPVarInt(playerid,"JobTime",15);
	//CallRemoteFunction("GiveAchievement", "iii", playerid, 2, 1);
	return 1;
}
ALTCOMMAND:vm->vehmenu;
CMD:vehmenu(playerid, params[])
{
    if(PlayerLogged[playerid] == false) SendErrorMessage(playerid, "You're not logged in.");
	if(GetPlayerState(playerid) != PLAYER_STATE_DRIVER) return SendClientMessage(playerid, COLOR_WHITE, "You must be in a vehicle to use this !");
	if(IsNotAEngineCar(GetPlayerVehicleID(playerid))) return SendClientMessage(playerid, COLOR_LIGHTRED, "This vehicle dosent have an engine !");
	if(PlayerInfo[playerid][pDead] > 0) return SendClientMessage(playerid, COLOR_WHITE, "Cannot use this command while dead.");
	//if(VehicleInfo[GetPlayerVehicleID(playerid)][vType] == 1 && GetPVarInt(playerid, "RentKey") != GetPlayerVehicleID(playerid)) return SendClientMessage(playerid, COLOR_GREY, "This vehicle is not a rentable vehicle.");
	new key = VehicleInfo[GetPlayerVehicleID(playerid)][vOwned];
	if(key != SCRIPT_CARS && PlayerInfo[playerid][pCarKey1] != key && PlayerInfo[playerid][pCarKey2] != key && PlayerInfo[playerid][pCarKey3] != key && VehicleInfo[GetPlayerVehicleID(playerid)][vEngine] == 0)  return SendClientMessage(playerid,COLOR_LIGHTRED,"WARNING: You don't have a key to this vehicle.");
	if(GetPVarInt(playerid, "Hotwire") > 0) return SendClientMessage(playerid,COLOR_LIGHTRED,"WARNING: You are currently busy.");
	new Float:ghealth, health;
	GetVehicleHealth(GetPlayerVehicleID(playerid),ghealth); health = floatround(ghealth);
	if(health <= 300) return SendClientMessage(playerid,COLOR_LIGHTRED,"Vehicle is currently damaged.");
    ShowPlayerDialog(playerid,DIALOG_VEHICLE,DIALOG_STYLE_LIST,"Vehicle Options","{00CED1}Toggle Engine\n{00CED1}Idle\n{00CED1}Lights\n{FF6347}Exit Vehicle","Continue", "Cancel");
	return 1;
}
CMD:pickupitem(playerid, params[])
{
    if(PlayerInfo[playerid][pDead] > 0) return SendClientMessage(playerid, COLOR_WHITE, "Cannot use this command while dead.");
	new interior = GetPlayerInterior(playerid), world = GetPlayerVirtualWorld(playerid);
	if(GetPlayerState(playerid) != PLAYER_STATE_ONFOOT) return 1;
	new f = MAX_OBJ+1;
	for(new a = 0; a < MAX_OBJ; a++)
	{
		if(IsPlayerInRangeOfPoint(playerid, 5.0, dGunData[a][ObjPos][0], dGunData[a][ObjPos][1], dGunData[a][ObjPos][2]))
		{
			if(interior == dGunData[a][ObjInt] && world == dGunData[a][ObjWorld])
			{
				f = a;
				break;
			}
		}
	}
	if(PlayerHaveSidarm(playerid,dGunData[f][ObjData][0])) return SendClientMessage(playerid,COLOR_GREY,"You already have a Secondary Weapon in your inventory.");
	if(f > MAX_OBJ) return SendClientMessage(playerid, COLOR_LIGHTRED, "There is no weapon on the ground here.");
    ApplyAnimation(playerid, "BOMBER", "BOM_Plant", 4.0, 0, 0, 0, 0, 0); // Place Bomb
	DestroyDynamicObject(dGunData[f][ObjID]);
	DestroyDynamic3DTextLabel(dGunData[f][ObjText]);
	GivePlayerWeaponEx(playerid, dGunData[f][ObjData][0], dGunData[f][ObjData][1]);
	dGunData[f][ObjPos][0] = 0.0;
	dGunData[f][ObjPos][1] = 0.0;
	dGunData[f][ObjPos][2] = 0.0;
	dGunData[f][ObjID] = -1;
	//dGunData[f][ObjData][0] = 0;
	dGunData[f][ObjData][1] = 0;
	dGunData[f][ObjInt] = 0;
	dGunData[f][ObjWorld] = 0;
	new buffer[50];
	format(buffer, sizeof(buffer), "You picked up a %s.", GunNames[dGunData[f][ObjData][0]]);
	SendClientMessage(playerid, -1, buffer);
	// -------------------------------------------------------
	return 1;
}
CMD:drop(playerid, params[])
{
	new type[128], amount, world = GetPlayerVirtualWorld(playerid), interior = GetPlayerInterior(playerid);
 	if(PlayerLogged[playerid] == false) SendErrorMessage(playerid, "You're not logged in.");
 	if(PlayerInfo[playerid][pCuffed] > 0) return SendErrorMessage(playerid, "You can't do this while handcuffed.");
	if(PlayerInfo[playerid][pJailed] > 0) return SendErrorMessage(playerid, "Cannot use this command while in-jail.");
	if(PlayerInfo[playerid][pDead] > 0) return SendErrorMessage(playerid, "Cannot use this command while dead.");
	if(sscanf(params, "s[128]I(-1)", type, amount)) SendClientMessage(playerid, COLOR_GREEN, "USAGE: /drop [bag/gun/weed]");
	else
	{
        if(strcmp(type, "bag", true) == 0)
	    {
            if(IsPlayerInAnyVehicle(playerid)) return SendClientMessage(playerid, COLOR_LIGHTRED, "You can't be in a vehicle while you use this !");
  	        {
  	            PlayerInfo[playerid][pBag] = 0;
  	        	RemovePlayerAttachedObject(playerid, HOLDBJECT_BAG);
  	        	ApplyAnimation(playerid, "BOMBER", "BOM_Plant", 4.0, 0, 0, 0, 0, 0); // Place Bomb
    		}
		}
    	else if(strcmp(type, "gun", true) == 0)
	    {
            if(GetPlayerWeapon(playerid) == 0) return SendClientMessage(playerid, COLOR_WHITE, "This is not a valid weapon.");
            if(PlayerWeapons[playerid][GetPlayerWeapon(playerid)] == 0) return SendClientMessage(playerid, COLOR_LIGHTRED, "The weapon you currently have in your hand is labeld a Hacked Weapon, You are either SA-MP Bugged or Cheating !");
            if(GetPlayerAmmo(playerid) == 0) return SendClientMessage(playerid, COLOR_WHITE, "You do not have enough ammo to drop this weapon.");
            if(IsPlayerInAnyVehicle(playerid)) return SendClientMessage(playerid, COLOR_LIGHTRED, "You can't be in a vehicle while you use this !");
            if(Tazer[playerid] != 0) return SendClientMessage(playerid, COLOR_WHITE, "You are not able to use this !");
            new GunID = GetPlayerWeapon(playerid);
		  	new GunAmmo = GetPlayerAmmo(playerid);
		   	if(GunID > 0 && GunAmmo != 0)
		    {
		    	new f = MAX_OBJ+1;
		     	for(new a = 0; a < MAX_OBJ; a++)
		      	{
		       		if(dGunData[a][ObjPos][0] == 0.0)
		         	{
		          		f = a;
		            	break;
		            }
				}
		  		if(f > MAX_OBJ) return SendClientMessage(playerid, COLOR_LIGHTRED, "You can not drop your gun at the moment. (max limit exceeded)");
		  		ApplyAnimation(playerid, "BOMBER", "BOM_Plant", 4.0, 0, 0, 0, 0, 0); // Place Bomb
				RemovePlayerWeaponEx(playerid, GunID);
				dGunData[f][ObjData][0] = GunID;
				dGunData[f][ObjData][1] = GunAmmo;

				dGunData[f][ObjInt] = interior;
				dGunData[f][ObjWorld] = world;
				
				dGunData[f][ObjType] = 1;

				GetPlayerPos(playerid, dGunData[f][ObjPos][0], dGunData[f][ObjPos][1], dGunData[f][ObjPos][2]);
				dGunData[f][ObjID] = CreateDynamicObject(GunobjectIds[GunID], dGunData[f][ObjPos][0], dGunData[f][ObjPos][1], dGunData[f][ObjPos][2]-1, 93.7, 120.0, 120.0, world, interior, -1, 200.0, 0.0);
				dGunData[f][ObjText] = CreateDynamic3DTextLabel("{339900}Item\n(/pickupitem)",0xFFFFFFFF, dGunData[f][ObjPos][0], dGunData[f][ObjPos][1], dGunData[f][ObjPos][2]-1, 10.0, INVALID_PLAYER_ID, INVALID_VEHICLE_ID, 0, world, interior, -1, 90);
				new buffer[50];
				format(buffer, sizeof(buffer), "You threw your %s on the ground.", GunNames[dGunData[f][ObjData][0]]);
				SendClientMessage(playerid, -1, buffer);
			}
		}
		else if(strcmp(type, "weed", true) == 0)
	    {
            //if(GetPlayerWeapon(playerid) == 0) return SendClientMessage(playerid, COLOR_WHITE, "This is not a valid weapon.");
            //if(GetPlayerAmmo(playerid) == 0) return SendClientMessage(playerid, COLOR_WHITE, "You do not have enough ammo to drop this weapon.");
            //if(IsPlayerInAnyVehicle(playerid)) return SendClientMessage(playerid, COLOR_LIGHTRED, "You can't be in a vehicle while you use this !");
            //if(Tazer[playerid] != 0) return SendClientMessage(playerid, COLOR_WHITE, "You are not able to use this !");
            //new amount = PlayerInfo[playerid][pWeed];
            if(PlayerInfo[playerid][pWeed] == 0) return SendClientMessage(playerid, COLOR_WHITE, "You don't have any weed on you.");
    		new f = MAX_OBJ+1;
	    	for(new a = 0; a < MAX_OBJ; a++)
    		{
   				if(dGunData[a][ObjPos][0] == 0.0)
    			{
    				f = a;
     				break;
         		}
			}
			if(f > MAX_OBJ) return SendClientMessage(playerid, COLOR_LIGHTRED, "You cannot drop your weed at the moment. (max limit exceeded)");
			ApplyAnimation(playerid, "BOMBER", "BOM_Plant", 4.0, 0, 0, 0, 0, 0); // Place Bomb
			dGunData[f][ObjInt] = interior;
			dGunData[f][ObjWorld] = world;
			dGunData[f][ObjData][1] = amount;
			PlayerInfo[playerid][pWeed] = 0;
			dGunData[f][ObjType] = 2;

			GetPlayerPos(playerid, dGunData[f][ObjPos][0], dGunData[f][ObjPos][1], dGunData[f][ObjPos][2]);
			dGunData[f][ObjID] = CreateDynamicObject(1578, dGunData[f][ObjPos][0], dGunData[f][ObjPos][1], dGunData[f][ObjPos][2]-1, 0.0, 0.0, 0.0, world, interior, -1, 200.0, 0.0);
			dGunData[f][ObjText] = CreateDynamic3DTextLabel("{339900}Item\n(/pickupitem)",0xFFFFFFFF, dGunData[f][ObjPos][0], dGunData[f][ObjPos][1], dGunData[f][ObjPos][2]-1, 10.0, INVALID_PLAYER_ID, INVALID_VEHICLE_ID, 0, world, interior, -1, 90);
			new buffer[50];
			format(buffer, sizeof(buffer), "You threw your %s on the ground.", GunNames[dGunData[f][ObjData][0]]);
			SendClientMessage(playerid, -1, buffer);
    	}
	}
	return 1;
}
