		            ShowPlayerDialog(playerid,DIALOG_SELL,DIALOG_STYLE_MSGBOX,"Selling Property",string,"Yes", "No");
          		}
   			}
		}
 		else if(strcmp(choose, "upgrade", true) == 0)
		{
		    if(PlayerInfo[playerid][HouseID] == 0) return SendErrorMessage(playerid, "You do not own a house!");
	        new key = PlayerInfo[playerid][HouseID];
	        if(strcmp(PlayerName(playerid), HouseInfo[key][hOwner], true) == 0)
	        {
	            if(IsPlayerInRangeOfPoint(playerid,50.0,HouseInfo[key][hExitX],HouseInfo[key][hExitY],HouseInfo[key][hExitZ]))
	            {
		    		ShowPlayerDialog(playerid,DIALOG_HOUSEUPGRADE,DIALOG_STYLE_LIST,"House Upgrade","{33FF66}Basic Essentials\n{33FF66}Radio plugin","Select", "Cancel");
	            }
                else SendClientMessage(playerid, COLOR_GREY, "You are not around your house interior.");
	        }
		}
		//else SendClientMessage(playerid, COLOR_GREY, "Invalid slot name.");
	}
	return 1;
}
