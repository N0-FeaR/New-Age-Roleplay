forward SaveNormalCars();
public SaveNormalCars()
{
    new idx, File: file2, coordsstring[256];
	while (idx < sizeof(NormalCar))
	{
		format(coordsstring, sizeof(coordsstring), "%s,%d,%d,%d,%d\n",
		NormalCar[idx][carname],
		NormalCar[idx][carprice],
		NormalCar[idx][carlevel],
		NormalCar[idx][carID],
		NormalCar[idx][carQ]);
		if(idx == 0) file2 = fopen("buyablecars/normalcars.ini", io_write);
		else file2 = fopen("buyablecars/normalcars.ini", io_append);
		if(!file2) return 1; // Do not saved a corrupt file.
		fwrite(file2, coordsstring);
		idx++;
		fclose(file2);
	}
	return 1;
}
