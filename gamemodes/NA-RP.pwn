CMD:editdoor(playerid, params[])
{
	new type[128],amount,doorid,string[228],Float:x,Float:y,Float:z,Float:a;
	GetPlayerPos(playerid,x,y,z);
	GetPlayerFacingAngle(playerid,a);
	if(PlayerLogged[playerid] == false) SendErrorMessage(playerid, "You're not logged in.");
	if(PlayerInfo[playerid][pAdmin] < 4) return SendErrorMessage(playerid, "You are not authorized to use this command.");
	if(sscanf(params, "s[128]dD(1)", type, doorid, amount)) SendSyntaxMessage(playerid, "/editdoor [exterior/interior/mapicon/custominterior/customoutside/remove] [doorid] [amount]");
	else
	{
	    if(doorid < 1 || doorid > 50) return SendClientMessage(playerid, COLOR_GREY, "Cannot go under 1 or above 50.");
 		if(strcmp(type, "exterior", true) == 0)
  		{
  			DoorInfo[doorid][dEntranceX] = x;
			DoorInfo[doorid][dEntranceY] = y;
			DoorInfo[doorid][dEntranceZ] = z;
			DoorInfo[doorid][dEntranceA] = a;
			DoorInfo[doorid][dInt] = GetPlayerInterior(playerid);
			DoorInfo[doorid][dWorld] = GetPlayerVirtualWorld(playerid);
			ReloadDoorIcon(doorid);
			format(string, sizeof(string), "WARNING: Moved Door ID: %d's exterior to %f,%f,%f,%f.", doorid, x, y, z, a);
			SendClientMessage(playerid, COLOR_LIGHTBLUE, string);
			SaveDoor(doorid);
			format(string, sizeof(string), "%s has changed DoorID: %d's exterior", PlayerName(playerid), doorid);
			Log("logs/editdoor.log", string);
 		}
  		else if(strcmp(type, "interior", true) == 0)
   		{
    		DoorInfo[doorid][dExitX] = x;
			DoorInfo[doorid][dExitY] = y;
			DoorInfo[doorid][dExitZ] = z;
			DoorInfo[doorid][dExitA] = a;
			DoorInfo[doorid][dInsideInt] = GetPlayerInterior(playerid);
			DoorInfo[doorid][dInsideWorld] = doorid+3000;
			format(string, sizeof(string), "WARNING: Moved Door ID: %d's interior to %f,%f,%f,%f.", doorid, x, y, z, a);
			SendClientMessage(playerid, COLOR_LIGHTBLUE, string);
			SaveDoor(doorid);
			format(string, sizeof(string), "%s has changed DoorID: %d's interior", PlayerName(playerid), doorid);
			Log("logs/editdoor.log", string);
		}
  		else if(strcmp(type, "mapicon", true) == 0)
   		{
   			if(DoorInfo[doorid][dIcon] > 0)
     		{
     			DestroyDynamicMapIcon(DoorInfo[doorid][dMapIcon]);
        	}
        	DoorInfo[doorid][dIcon] = amount;
         	format(string, sizeof(string), "Edited DoorID: %d's %s to %d.", doorid, type, amount);
          	SendClientMessage(playerid,COLOR_LIGHTBLUE,string);
           	if(DoorInfo[doorid][dIcon] > 0)
           	{
				DoorInfo[doorid][dMapIcon] = CreateDynamicMapIcon(DoorInfo[doorid][dEntranceX], DoorInfo[doorid][dEntranceY], DoorInfo[doorid][dEntranceZ], DoorInfo[doorid][dIcon], COLOR_YELLOW);
 			}
    		SaveDoor(doorid);
			format(string, sizeof(string), "%s has changed DoorID: %d's mapicon", PlayerName(playerid), doorid);
			Log("logs/editdoor.log", string);
		}
  		else if(strcmp(type, "custominterior", true) == 0)
   		{
   		    DoorInfo[doorid][dCustomInterior] = amount;
   		    format(string, sizeof(string), "Edited DoorID: %d's %s to %d.", doorid, type, amount);
          	SendClientMessage(playerid,COLOR_LIGHTBLUE,string);
   		    format(string, sizeof(string), "%s has changed DoorID: %d's custominterior", PlayerName(playerid), doorid);
			Log("logs/editdoor.log", string);
		}
  		else if(strcmp(type, "customoutside", true) == 0)
   		{
   		    DoorInfo[doorid][dCustomOutside] = amount;
   		    format(string, sizeof(string), "Edited DoorID: %d's %s to %d.", doorid, type, amount);
          	SendClientMessage(playerid,COLOR_LIGHTBLUE,string);
   		    format(string, sizeof(string), "%s has changed DoorID: %d's customoutside", PlayerName(playerid), doorid);
			Log("logs/editdoor.log", string);
		}
  		else if(strcmp(type, "remove", true) == 0)
   		{
   			DoorInfo[doorid][dEntranceX] = 0;
			DoorInfo[doorid][dEntranceY] = 0;
   			DoorInfo[doorid][dEntranceZ] = 0;
	    	DoorInfo[doorid][dEntranceA] = 0;
		    DoorInfo[doorid][dExitX] = 0;
		    DoorInfo[doorid][dExitY] = 0;
		    DoorInfo[doorid][dExitZ] = 0;
		    DoorInfo[doorid][dExitA] = 0;
		    DoorInfo[doorid][dInt] = 0;
		    DoorInfo[doorid][dWorld] = 0;
		    DestroyDynamicMapIcon(DoorInfo[doorid][dMapIcon]);
			    
		    DestroyDynamic3DTextLabel(DoorInfo[doorid][dLabel]);
			DestroyDynamicPickup(DoorInfo[doorid][dPickup]);

			format(string, sizeof(string), DPATH, doorid);
			fremove(string);
    			
			SendServerMessage(playerid, "You removed Door ID: %d", doorid);
			format(string, sizeof(string), "%s Have removed door id: %d", PlayerName(playerid), doorid);
			Log("logs/editdoor.log", string);
			return 1;
   		}
     	else
      	{
     		format(string, sizeof(string), "Door ID: %d.", doorid);
     		SendClientMessage(playerid,COLOR_GREY,string);
		}
	}
	return 1;
}
CMD:createdoor(playerid, params[])
{
	new
	name[128], id, int, world, Float:Xi,
	Float:Yi, Float:Zi, inti, Float:X,Float:Y,Float:Z,Float:A;
	if(PlayerLogged[playerid] == false) SendErrorMessage(playerid, "You're not logged in.");
    if(PlayerInfo[playerid][pAdmin] < 4) return SendErrorMessage(playerid, "You're not authorized to use this command.");
    if(sscanf(params, "s[128]", name)) return SendSyntaxMessage(playerid, "/createdoor [name]");

    for(new d = 1;d < sizeof(DoorInfo);d++)
    {
    	id = d;
     	break;
    }

    strmid(DoorInfo[id][dName], name, 0, strlen(name), 255);
    GetPlayerPos(playerid, X, Y, Z);
    GetPlayerFacingAngle(playerid, A);
    int = GetPlayerInterior(playerid);
    world = GetPlayerVirtualWorld(playerid);
    DoorInfo[id][dInsideInt] = inti;
    DoorInfo[id][dExitX] = Xi;
    DoorInfo[id][dExitY] = Yi;
    DoorInfo[id][dExitZ] = Zi;

    DoorInfo[id][dEntranceX] = X;
    DoorInfo[id][dEntranceY] = Y;
    DoorInfo[id][dEntranceZ] = Z;
    DoorInfo[id][dEntranceA] = A;


    DoorInfo[id][dInt] =int;
    DoorInfo[id][dWorld] =world;
    DoorInfo[id][dInsideWorld] =id;
    
    CreateDynamic3DTextLabel("{FFFFFF}[{339900}Door{FFFFFF}]",0x00AE00FF,DoorInfo[id][dEntranceX], DoorInfo[id][dEntranceY], DoorInfo[id][dEntranceZ]+0.75, 25.0, INVALID_PLAYER_ID, INVALID_VEHICLE_ID, 1, DoorInfo[id][dWorld], DoorInfo[id][dInt], -1, 100.0);
	CreateDynamicPickup(1318, 1, DoorInfo[id][dEntranceX], DoorInfo[id][dEntranceY], DoorInfo[id][dEntranceZ], DoorInfo[id][dWorld], 0);

    new file4[40];
    format(file4, sizeof(file4), DPATH, id);
    new INI:File = INI_Open(file4);
    INI_SetTag(File,"data");
    INI_WriteString(File,"Name", DoorInfo[id][dName]);
    INI_WriteFloat(File,"EntranceX", DoorInfo[id][dEntranceX]);
    INI_WriteFloat(File,"EntranceY", DoorInfo[id][dEntranceY]);
    INI_WriteFloat(File,"EntranceZ", DoorInfo[id][dEntranceZ]);
    INI_WriteFloat(File,"EntranceA", DoorInfo[id][dEntranceA]);
    INI_WriteFloat(File,"ExitX", DoorInfo[id][dExitX]);
    INI_WriteFloat(File,"ExitY", DoorInfo[id][dExitY]);
    INI_WriteFloat(File,"ExitZ", DoorInfo[id][dExitZ]);
    INI_WriteFloat(File,"ExitA", DoorInfo[id][dExitA]);
    INI_WriteInt(File,"Int", DoorInfo[id][dInt]);
    INI_WriteInt(File,"World", DoorInfo[id][dWorld]);
    INI_WriteInt(File,"InsideInt", DoorInfo[id][dInsideInt]);
    INI_WriteInt(File,"InsideWorld", DoorInfo[id][dInsideWorld]);
    INI_WriteInt(File,"Icon", DoorInfo[id][dIcon]);
    INI_WriteInt(File,"CustonInterior", DoorInfo[id][dCustomInterior]);
    INI_WriteInt(File,"CustomOutside", DoorInfo[id][dCustomOutside]);
    INI_Close(File);
    return 1;
}
