        format(string, sizeof(string),"Name: %s - Date: %d-%d-%d.", sendername, day, month, year);
        SendClientMessage(playerid,0xFF8282AA,string);
	    format(string, sizeof(string),"Medical Bill: $%d - Tax: $%d.", bill, tax);
        SendClientMessage(playerid,0xFF8282AA,string);
