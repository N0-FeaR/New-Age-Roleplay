CMD:forbid(playerid, params[])
{
	new text[128],time,TargetID,string[256],sendername[MAX_PLAYER_NAME],giveplayer[MAX_PLAYER_NAME];
	if(GMX[playerid] == 1) return SendErrorMessage(playerid, "You cannot use this right now!");
	if(sscanf(params, "uis[128]", TargetID, time, text)) SendClientMessage(playerid, COLOR_GREY, "USAGE: /forbid [playerid] [time(minutes)] [reason]");
	else
	{
		if(PlayerLogged[playerid] == false) SendErrorMessage(playerid, "You're not logged in.");
	    if (!IsPlayerConnected(TargetID)) return SendClientMessage(playerid, COLOR_WHITE, "That player is not connected !");
	    if(IsPlayerNPC(TargetID)) return SendClientMessage(playerid, COLOR_LIGHTRED, "Can't do this to a NPC.");
	    if(strlen(text) >= 100) return SendClientMessage(playerid, COLOR_WHITE, "Reason is too long.");
		if(PlayerInfo[playerid][pAdmin] < 1) return SendErrorMessage(playerid, "You do not have access to this command!");
		{
			PlayerInfo[TargetID][pForbidWeapon] = 1;
			PlayerInfo[TargetID][pForbidWeaponTime] =time*60;
			format(string, sizeof(string), "You are forbid to carry weapons for %d minutes.", time);
			SendClientMessage(TargetID, COLOR_WHITE, string);
			format(string, sizeof(string), "REASON: %s.", text);
			SendClientMessage(TargetID, COLOR_WHITE, string);
			format(string, sizeof(string), "AdmCmd: %s was admin forbid for carry weapons by Admin %s Reason:[%s]", giveplayer, sendername, text);
			Log("logs/forbid.log", string);
		}
	}
	return 1;
}
