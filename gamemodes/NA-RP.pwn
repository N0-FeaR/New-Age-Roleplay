stock CreateFire(Float:X, Float:Y, Float:Z, World, Interior)
{
    new rand = random(5)+1, rand2 = random(5)+1, Float:adjustment[2], area = 0;
    if(X > 46.7115 && Y > -2755.979 && X < 2931.147 && Y < -548.8602) area++;
    if(area == 0) return true; // Block fires not in NA-RP district.
    switch(rand)
    {
        case 1: adjustment[0]=1.0;
        case 2: adjustment[0]=2.0;
        case 3: adjustment[0]=3.5;
        case 4: adjustment[0]=2.0;
        case 5: adjustment[0]=2.2;
        default: adjustment[0]=3.0;
    }
    switch(rand2)
    {
        case 1: adjustment[1]=1.0;
        case 2: adjustment[1]=2.0;
        case 3: adjustment[1]=3.5;
        case 4: adjustment[1]=2.0;
        case 5: adjustment[1]=2.1;
        default: adjustment[1]=3.0;
    }
    CreateFireEx(X, Y, Z, World, Interior);
    CreateFireEx(X+adjustment[0], Y-adjustment[1], Z, World, Interior);
    CreateFireEx(X+adjustment[0], Y+adjustment[1], Z, World, Interior);
    CreateFireEx(X-adjustment[0], Y+adjustment[1], Z, World, Interior);
    CreateFireEx(X-adjustment[0], Y-adjustment[1], Z, World, Interior);
    return true;
}
