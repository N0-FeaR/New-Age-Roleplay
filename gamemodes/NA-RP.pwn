public OnPlayerUpdate(playerid)
{

    if(!IsPlayerConnected(playerid)) return 0;
	new keys, updown, leftright, string[128],animlib[128],animname[128],
	Float:X, Float:Y, Float:Z, Float:ghealth,
	Float:health,Float:armour, Float:speed = GetPlayerSpeed(playerid, false);
	GetPlayerPos(playerid,X,Y,Z);
    GetPlayerKeys(playerid,keys,updown,leftright); GetPlayerHealth(playerid,health);
	GetPlayerArmour(playerid,armour); GetAnimationName(GetPlayerAnimationIndex(playerid),animlib,128,animname,128);
	if(IsPlayerNPC(playerid)) return 1;
	// Anti-Running system with handcuffs.
	if(GetPlayerSpecialAction(playerid) == SPECIAL_ACTION_CUFFED && GetPlayerState(playerid) == PLAYER_STATE_ONFOOT && GetPVarInt(playerid, "Control") == 0 && speed >= 7.0)
	{
		ClearAnimations(playerid);
        ApplyAnimation(playerid, "ped", "FLOOR_hit_f", 4.0, 0, 1, 1, 1, -1);
	    SetTimerEx("PlayerGetup", 5000, false, "i", playerid);
        return 0;
	}
	// Anti Jet-Pack Hack.
	if(GetPlayerSpecialAction(playerid) == SPECIAL_ACTION_USEJETPACK && PlayerInfo[playerid][pAdmin] == 0)
    {
        SendAdminAction(playerid, "%s was banned by Anna-Bot Reason:[Jetpack Hack].", PlayerName(playerid));
        return 0;
    }
    if(IsPlayerInAnyVehicle(playerid) && !IsNotAEngineCar(GetPlayerVehicleID(playerid)))
	{
    	switch(VehicleInfo[GetPlayerVehicleID(playerid)][vFuel])
	    {
		    case 1:
			{
			    TextDrawHideForPlayer(playerid,UsedDraw[playerid]);
			    TextDrawHideForPlayer(playerid,VehicleDraw[0]);
			    TextDrawHideForPlayer(playerid,VehicleDraw[1]);
                TextDrawHideForPlayer(playerid,VehicleDraw[2]);
			    TextDrawHideForPlayer(playerid,VehicleDraw[3]);
                TextDrawHideForPlayer(playerid,VehicleDraw[4]);
	            TextDrawHideForPlayer(playerid,VehicleIDraw[playerid]);
				SendClientMessage(playerid,COLOR_LIGHTRED,"WARNING: There is no more fuel in you're vehicle type (/exitcar) to exit.");
				VehicleInfo[GetPlayerVehicleID(playerid)][vFuel] = 0, VehicleInfo[GetPlayerVehicleID(playerid)][vEngine] = 0;
		    	CarEngine(GetPlayerVehicleID(playerid),0), CarLights(GetPlayerVehicleID(playerid),2);
		    	return 0;
		    }
			case 2 .. 300:
			{
			    new vhealth = floatround(ghealth);
				switch(vhealth)
				{
				    case 1 .. 300:
					{
					    if(VehicleInfo[GetPlayerVehicleID(playerid)][vEngine] == 1 && GetPlayerState(playerid) == PLAYER_STATE_DRIVER)
						{
		    			    TextDrawHideForPlayer(playerid,UsedDraw[playerid]);
			    		    TextDrawHideForPlayer(playerid,VehicleDraw[0]);
		                    TextDrawHideForPlayer(playerid,VehicleDraw[1]);
                		    TextDrawHideForPlayer(playerid,VehicleDraw[2]);
			    		    TextDrawHideForPlayer(playerid,VehicleDraw[3]);
                		    TextDrawHideForPlayer(playerid,VehicleDraw[4]);
	            		    TextDrawHideForPlayer(playerid,VehicleIDraw[playerid]);
		   					switch(VehicleInfo[GetPlayerVehicleID(playerid)][vType])
							{
			    			    case 1:
			    			    {
			        		        if(GetPVarInt(playerid, "RentKey") == GetPlayerVehicleID(playerid) && GetPVarInt(playerid, "RentCar") < 5000)
				    		        {
				        	            SetPVarInt(playerid, "RentCar", GetPVarInt(playerid, "RentCar")+500), format(string, sizeof(string),"You now owe $%d to the Los Santos Rental Company.",GetPVarInt(playerid, "RentCar")), SendClientMessage(playerid,COLOR_LIGHTRED,string);
			        		        }
			    			    }
							    case 8, 10, 11, 13, 14, 16, 17, 18:
							    {
				    		        if(GetPVarInt(playerid, "Job") > 0 && GetPVarInt(playerid, "JobCar") < 5000)
   	    				            {
				        	            SetPVarInt(playerid, "JobCar", GetPVarInt(playerid, "JobCar")+500), format(string, sizeof(string),"You now owe $%d to your Job Company.",GetPVarInt(playerid, "JobCar")), SendClientMessage(playerid,COLOR_LIGHTRED,string);
			        		        }
							    }
							}
		    				if(VehicleInfo[GetPlayerVehicleID(playerid)][vFuel] > 20) VehicleInfo[GetPlayerVehicleID(playerid)][vFuel] = 20;
		    				VehicleInfo[GetPlayerVehicleID(playerid)][vEngine] = 0, CarEngine(GetPlayerVehicleID(playerid),0), CarLights(GetPlayerVehicleID(playerid),2);
    	    			    format(string, sizeof(string), "The %ss engine broke-down.", VehicleNames[GetVehicleModel(GetPlayerVehicleID(playerid))-400]), SendClientMessage(playerid, 0xCCEEFF96, string);
    	    		    }
				    }
		        }
		        PrintSpeedo(playerid, speed);
		    }
	    }
    }
	return 1;
}
public OnPlayerGiveDamage(playerid, damagedid, Float:amount, weaponid)
{
	if(Tazer[playerid] == 1 && GetPlayerWeapon(playerid) == TAZE_WEAPON)
	{
	    new Float:health,losehp;
    	GetPlayerHealth(damagedid,health);
    	losehp = TAZE_LOSEHP;
	    if(Tazed[damagedid] == 1) return 1;
	    new Float:x, Float:y, Float:z;
	    GetPlayerPos(damagedid, x, y, z);
	    ClearAnimations(damagedid);
	    ApplyAnimation(damagedid,"PED","KO_skid_front",4.1,0,1,1,1,0);
	    Spark[damagedid] = CreateObject(TAZE_SPARK, x, y, z-3, 0, 0, 0);
	    SetTimerEx("DestroySpark", TAZE_DESTROY, 0, "i", damagedid);
	    SetTimerEx("TazedRemove", TAZE_TIMER, 0, "i", damagedid);
	    //TogglePlayerControllable(damagedid, 0);
	    Tazed[damagedid] = 1;
	    if(losehp) {} else SetPlayerHealthEx(damagedid, health+amount);
	}
	return 1;
}
