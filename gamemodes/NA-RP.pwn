CMD:makeleader(playerid, params[])
{
	new id, TargetID;
	if(PlayerLogged[playerid] == false) SendErrorMessage(playerid, "You're not logged in.");
    if(PlayerInfo[playerid][pFactionMod] >= 1 || PlayerInfo[playerid][pAdmin] >= 5)
    if(sscanf(params, "ud", TargetID, id)) return SendSyntaxMessage(playerid, "Usage: /makeleader [playerid] [factionID]");
    else
	{
	    if(!IsPlayerConnected(TargetID)) return SendErrorMessage(playerid, "That player is not connected.");
		if(id < 1 || id > 10 ) return SendErrorMessage(playerid, "Invalid faction ID.");
		if(IsPlayerNPC(TargetID)) return SendErrorMessage(playerid, "Can't do this to a NPC.");

		FactionInfo[id][fLeader] = PlayerName(TargetID);
		//PlayerInfo[TargetID][fRank] = 16;
		PlayerInfo[TargetID][fRank] = 16;
		PlayerInfo[TargetID][pFaction] = id;
		FactionInfo[id][fMembers] += 1;
	 	new file4[40];
	 	format(file4, sizeof(file4), FPATH, id);
	 	new INI:File = INI_Open(file4);
	 	INI_SetTag(File,"data");
	 	INI_WriteString(File,"Leader", FactionInfo[id][fLeader]);
	 	INI_WriteInt(File,"Members", FactionInfo[id][fMembers]);
	 	INI_Close(File);
	 	switch(id)
		{
			case 0: PlayerInfo[TargetID][fRank] = 0;
			case 1: PlayerInfo[TargetID][fRank] = 16, PlayerInfo[TargetID][pSwat] = 1;
			case 2: PlayerInfo[TargetID][fRank] = 15;
		}
		SendServerMessage(playerid, "You have set %s as the leader for %s.", PlayerName(TargetID), FactionInfo[id][fName]);
	}
	else return SendErrorMessage(playerid, "You're not authorized to use this command.");
    return 1;
}
