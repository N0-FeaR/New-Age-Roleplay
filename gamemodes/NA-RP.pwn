	                    TextDrawHideForPlayer(playerid,MoneyDraw2);
	                    ClearChatbox(playerid, 10);
	                    FadeTime[playerid]=255;
	                    FadeScreen(playerid, 2);
   	                    OnPlayerSpawn(playerid);
	                }
	                if(GetPVarInt(playerid,"DeathToSpawn") == 1)
	                {
	                    DeletePVar(playerid, "DeathToSpawn");
	                    FadeTime[playerid]=255;
	                    FadeScreen(playerid, 2);
	                    ClearChatbox(playerid, 10);
	                    StopAudioStreamForPlayer(playerid);
	                    AfterSpawnHos(playerid);
	                }
	            }
	            case 2:
	            {
	                FadeTime[playerid]=255;
	                FadeScreen(playerid, 2);
	                ClearChatbox(playerid, 10);
                    //LoadPlayerSpawnData(playerid);
	            }
	        }
	    }
	    case 2: // From Fading Out
	    {
	        // Code here!
	    }
	}
	return 1;
}
