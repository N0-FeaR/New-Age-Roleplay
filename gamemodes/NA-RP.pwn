        INI_ParseFile(filestring, "LoadIP_%s", .bExtra = true , .extra = playerid);
        new cmdstring[44];
        format(cmdstring, sizeof(cmdstring), "unbanip %s", PlayerInfo[playerid][pBanIP]);
        SendRconCommand(cmdstring);
        SendRconCommand("reloadbans");
        new done[128];
        format(done, sizeof(done),"AdmCmd: You have successfully unbanned %s.", targetname);
        SendClientMessage(playerid, COLOR_LIGHTRED,done);
  		format(string, sizeof(string), "%s has been unbanned by admin %s", targetname, PlayerName(playerid));
		Log("logs/unban.log", string);
    }
    return 1;
}
