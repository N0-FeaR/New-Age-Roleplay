    if(PlayerInfo[playerid][pDead] > 0) return SendClientMessage(playerid, COLOR_WHITE, "Cannot use this command while dead.");
	new choose[128],key = PlayerInfo[playerid][HouseID],hworld = GetPlayerVirtualWorld(playerid);
	if(sscanf(params, "s[128]", choose)) return SendClientMessage(playerid, COLOR_GREY, "USAGE: /light {FFFFFF}[on/off]");
	else
	{
		if(hworld == HouseInfo[key][hInsideWorld])
  		{
			if(HouseInfo[key][hElectric] == 0) return SendClientMessage(playerid, COLOR_GREY, "This house doesn't have Electric so you cannot trun on the light!");
			if(strcmp(choose, "on", true) == 0)
			{
				HouseInfo[key][hLights]=1;
		 		PlayerTextDrawHide(playerid,Electric0);
	            SendNearbyMessage(playerid, 30.0, COLOR_PURPLE, "*** %s Press the switch and turn the lights on",  PlayerName(playerid));
			}
			else if(strcmp(choose, "off", true) == 0)
			{
				HouseInfo[key][hLights]=0;
		 		PlayerTextDrawShow(playerid,Electric0);
				SendNearbyMessage(playerid, 30.0, COLOR_PURPLE, "*** %s Press the switch and turn the lights off",  PlayerName(playerid));
			}
		}
	}
	return 1;
}
CMD:takejob(playerid, params[])
{
    if(PlayerLogged[playerid] == false) SendErrorMessage(playerid, "You're not logged in.");
    if(PlayerInfo[playerid][pJob] != 0) return SendClientMessage(playerid, COLOR_WHITE, "You already have a job, /quitjob.");
    if(PlayerInfo[playerid][pDriveLic] == 0) return SendClientMessage(playerid, COLOR_WHITE, "You need a Driver-License before applying for a job.");
    if(GetPVarInt(playerid, "JobTime") > 0)
    {
		new string[128];
		format(string, sizeof(string),"You must wait %d minutes before quiting your job.",GetPVarInt(playerid, "JobTime"));
		return SendClientMessage(playerid, COLOR_WHITE, string);
    }
    new string[128];
    for(new h = 0; h < sizeof(Jobs); h++)
	{
	    if(IsPlayerInRangeOfPoint(playerid,2.0,Jobs[h][0],Jobs[h][1],Jobs[h][2]))
	    {
	        format(string, sizeof(string),"You are now a %s, use /quitjob to leave and /help for commands.",JobNames[h]);
	        SendClientMessage(playerid, COLOR_WHITE, string);
	        PlayerInfo[playerid][pJob] = h+1;
	        //SetPVarInt(playerid,"JobTime",15);
	        CallRemoteFunction("GiveAchievement", "iii", playerid, 1, 1);
	    }
    }
	return 1;
}
