CMD:suspect(playerid, params[])
{
	new TargetID, crime[128], string[128];
	if(PlayerInfo[playerid][pFaction] != 1) return SendErrorMessage(playerid, "You are not a police officer.");
	if(sscanf(params, "us[128]", TargetID, crime)) SendSyntaxMessage(playerid, "/suspect [playerid] [crime description]");
	else
	{
	    if(IsPlayerNPC(TargetID)) return SendErrorMessage(playerid, "Can't do this to a NPC.");
	    if(PlayerLogged[playerid] == false) SendErrorMessage(playerid, "You're not logged in.");
	    if(playerid == TargetID) return SendClientMessage(playerid, COLOR_GREY, "You cannot suspect yourself.");
	    if(!IsPlayerConnected(TargetID)) return SendErrorMessage(playerid, "That player is not connected !");
		{
        	format(string, sizeof(string), "HQ: All Units APB: Reporter: %s", PlayerName(playerid));
            SendFactionMessage(1, COLOR_BLUE, string);
			format(string, sizeof(string), "HQ: Crime: %s, Suspect: %s", crime, PlayerName(TargetID));
			SendFactionMessage(1, COLOR_BLUE, string);
			strmid(PlayerInfo[TargetID][pLastCrime], crime, 0, strlen(crime), 255);
			PlayerInfo[TargetID][pWantedLevel] += 1;
			SetPlayerWantedLevel(TargetID,PlayerInfo[TargetID][pWantedLevel]);
			PlayerInfo[TargetID][pCrime] += 1;
			format(string, sizeof(string), "%s,%s", PlayerName(TargetID), crime);
			DatabaseNameAdd(string, "arrests.ini");
		}
	}
	return 1;
}
CMD:setrank(playerid, params[])
{
	new TargetID, rank;
