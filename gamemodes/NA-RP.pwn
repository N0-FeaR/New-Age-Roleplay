CMD:trunk(playerid, params[])
{
	new type[128],amount,string[158],carid,key,sendername[MAX_PLAYER_NAME],gunname[100];
	if(sscanf(params, "s[128]I(-1)", type,amount))
	{
	    SendClientMessage(playerid, COLOR_GREEN, "USAGE: /trunk [open/close/info/takegun/putgun/takearmour/putarmour/putweed/takeweed]");
	    SendClientMessage(playerid, COLOR_GREEN, "USAGE: /trunk [putcocaine/takecocaine/putcrack/takecrack/putmeth/takemeth]");
	    SendClientMessage(playerid, COLOR_GREEN, "USAGE: /trunk [akegascan/putgascan]");
	}
	else
	{
	    if(PlayerLogged[playerid] == false) SendErrorMessage(playerid, "You're not logged in.");
	    if(GetPlayerState(playerid) != PLAYER_STATE_ONFOOT) return SendClientMessage(playerid, COLOR_GREY, "You must be on-foot.");
	    if(!PlayerToCar(playerid,1,4.0)) return SendClientMessage(playerid, COLOR_LIGHTRED, "WARNING: You are not close to any vehicle.");
	    carid = PlayerToCar(playerid,2,4.0);
	    key = VehicleInfo[carid][vOwned];
	    if(IsNotAEngineCar(carid)) return SendClientMessage(playerid, COLOR_GREY, "This vehicle dosent have an engine.");
	    if(VehicleInfo[carid][vType] != 2 && VehicleInfo[carid][vType] != 3) return SendClientMessage(playerid, COLOR_GREY, "This is not an ownable/LSPD vehicle.");
	    if(VehicleInfo[carid][vType] == 3 && PlayerInfo[playerid][pFaction] != 1) return SendClientMessage(playerid, COLOR_GREY, "Trunk is locked.");
        if(strcmp(type, "open", true) == 0)
	    {
	        if(VehicleInfo[carid][vType] == 2)
	        {
	            if(key == PlayerInfo[playerid][pCarKey1]){}
                else if(key == PlayerInfo[playerid][pCarKey2]){}
                else if(key == PlayerInfo[playerid][pCarKey3]){}
                else if(PlayerInfo[playerid][pFaction] == 1 && GetPVarInt(playerid, "Rank") >= 7){}
                else
                {
                    return SendClientMessage(playerid, COLOR_GREY, "Trunk is locked.");
                }
            }
			if(VehicleInfo[carid][vType] == 2) CarInfo[key][cTOpen]=1;
			else VehicleInfo[carid][vTOpen]=1;
            GetVehicleParamsEx(carid,engine,lights,alarm,doors,bonnet,boot,objective);
            SetVehicleParamsEx(carid,engine,lights,alarm,doors,bonnet,1,objective);
            VehicleInfo[carid][vTDelay] = 2;
            if(!IsHelmetVehicle(carid)) ApplyAnimation(playerid, "CARRY", "liftup", 3.0, 0, 0, 0, 0, 0);
	    }
	    else if(strcmp(type, "close", true) == 0)
	    {
			if(VehicleInfo[carid][vType] == 2) CarInfo[key][cTOpen]=0;
			else VehicleInfo[carid][vTOpen]=0;
            GetVehicleParamsEx(carid,engine,lights,alarm,doors,bonnet,boot,objective);
            SetVehicleParamsEx(carid,engine,lights,alarm,doors,bonnet,0,objective);
            VehicleInfo[carid][vTDelay]=0;
            if(!IsHelmetVehicle(carid)) ApplyAnimation(playerid, "CARRY", "putdwn", 3.0, 0, 0, 0, 0, 0);
	    }
        else if(strcmp(type, "info", true) == 0)
	    {
	        if(VehicleInfo[carid][vType] == 2)
			{
			    if(CarInfo[key][cTOpen] == 0) return SendClientMessage(playerid,COLOR_GREY,"Trunk is closed (/trunk open).");
	            SendClientMessage(playerid, COLOR_LIGHTBLUE, "|_______ Vehicle Trunk _______|");
	            if(IsBike(carid))
	            {
			        if(CarInfo[key][cTWeapon][1] != 0)
			    	{
				        GetWeaponName(CarInfo[key][cTWeapon][1], gunname, sizeof(gunname));
				        format(string, sizeof(string), "| Slot 1: %s (Ammo: %i)", gunname, CarInfo[key][cTAmmo][1]);
				        SendClientMessage(playerid, COLOR_WHITE, string);
				    }
				    else
				    {
                        SendClientMessage(playerid, COLOR_WHITE,"| Slot 1: Empty (Ammo: N/A)");
				    }
	            }
	            else
	            {
	                for(new slot = 1; slot != 5; slot++)
			        {
			            if(CarInfo[key][cTWeapon][slot] != 0)
			    	    {
				            GetWeaponName(CarInfo[key][cTWeapon][slot], gunname, sizeof(gunname));
				            format(string, sizeof(string), "| Slot %i: %s (Ammo: %i)", slot, gunname, CarInfo[key][cTAmmo][slot]);
				            SendClientMessage(playerid, COLOR_WHITE, string);
				        }
				        else
				        {
				            format(string, sizeof(string), "| Slot %i: Empty (Ammo: N/A)", slot);
				            SendClientMessage(playerid, COLOR_WHITE, string);
				        }
			        }
	            }
			    format(string, sizeof(string), "| Weed: %d - Cocaine: %d - Crack: %d - Meth %d", CarInfo[key][cTWeed],CarInfo[key][cTCocaine],CarInfo[key][cTCrack],CarInfo[key][cTMeth]);
			    SendClientMessage(playerid, COLOR_WHITE, string);
			    format(string, sizeof(string), "| Armour: %.1f", CarInfo[key][cTArmour]);
			    SendClientMessage(playerid, COLOR_WHITE, string);
			    if(CarInfo[key][cGasCan] == 1) SendClientMessage(playerid, COLOR_WHITE, "| Gascan");
			    foreach (Player,i)
			    {
			        if(GetPVarInt(i, "InTrunk") == carid)
			        {
			            format(string, sizeof(string), "| %s is inside the trunk.", PlayerName(i));
			            SendClientMessage(playerid, COLOR_WHITE, string);
			        }
			    }
			}
			else
			{
