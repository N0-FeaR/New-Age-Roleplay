CMD:takegun(playerid, params[])
{
    new string[128],gunname[128];
   	if(PlayerLogged[playerid] == false) SendErrorMessage(playerid, "You're not logged in.");
    if(PlayerInfo[playerid][pInvWeapon] == 0) return SendClientMessage(playerid, COLOR_GREY, "You don't have a weapon in your inventory.");
    if(GetPVarInt(playerid, "Delay") > GetCount()) return SendClientMessage(playerid, COLOR_LIGHTRED, "You must wait before performing this command !");
    //if(GetPVarInt(playerid, "LSPD_Ta") != 0) return SendClientMessage(playerid, COLOR_WHITE, "You are not able to use this !");
    if(GetPlayerState(playerid) == PLAYER_STATE_DRIVER)
    {
        if(PlayerInfo[playerid][pInvWeapon] == 28 || PlayerInfo[playerid][pInvWeapon] == 29 || PlayerInfo[playerid][pInvWeapon] == 32)
        {
            return SendClientMessage(playerid, COLOR_LIGHTRED, "You can't 'takegun' this type of weapon.");
        }
    }
    if(GetPlayerState(playerid) == PLAYER_STATE_PASSENGER)
    {
        if(PlayerInfo[playerid][pInvWeapon] == 24) return SendClientMessage(playerid, COLOR_LIGHTRED, "You can't 'takegun' this type of weapon.");
    }
    if(PlayerInfo[playerid][pInvWeapon] == 43) GivePlayerWeaponEx(playerid, PlayerInfo[playerid][pInvWeapon], 9999999);
    else GivePlayerWeaponEx(playerid,PlayerInfo[playerid][pInvWeapon],PlayerInfo[playerid][pInvAmmo]);
    switch(PlayerInfo[playerid][pInvWeapon])
	{
	    case 34:
		{
		    if(GetPVarInt(playerid, "Member") != 7 && GetPVarInt(playerid, "Admin") == 0)
		    {
		        SendAdminAction(playerid, "%s has equiped a Sniper Rifle.", PlayerName(playerid));
            }
        }
	}
    GetWeaponName(PlayerInfo[playerid][pInvWeapon], gunname, sizeof(gunname));
    format(string, sizeof(string),"%s taken from your inventory !",gunname);
    SendClientMessage(playerid, COLOR_WHITE, string);
	PlayerInfo[playerid][pInvWeapon] = 0;
	PlayerInfo[playerid][pInvAmmo] = 0;
    SetPVarInt(playerid, "Delay", GetCount()+3000);
    RemovePlayerAttachedObject(playerid, HOLDBJECT_GUN2);
