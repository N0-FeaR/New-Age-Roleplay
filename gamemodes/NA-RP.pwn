CMD:ban(playerid, params[])
{
	new TargetID, reason[64], string[228], plrIP[16];
	if(PlayerInfo[playerid][pAdmin] < 2) return SendErrorMessage(playerid, "You're not authorized to use this command.");
	if(sscanf(params, "us[64]", TargetID,reason)) return SendSyntaxMessage(playerid, "/ban [playerid] [reason]");
	if(IsPlayerNPC(TargetID)) return SendErrorMessage(playerid, "Can't do this to a NPC.");
	if(!IsPlayerConnected(TargetID)) return SendClientMessage(playerid, COLOR_GREY, "That player is not connected.");
	if(PlayerInfo[TargetID][pAdmin] > PlayerInfo[playerid][pAdmin]) return SendErrorMessage(playerid, "You cant perform this on Admins that are higher than your level!");
  	GetPlayerIp(playerid, plrIP, sizeof(plrIP));
	format(string, sizeof(string), "Your name: %s\nYour ip is: %s\nYou were banned by: %s\nYou were banned for: %s", PlayerName(TargetID), plrIP, PlayerName(playerid), reason);
	ShowPlayerDialog(playerid, DIALOG_BAN, DIALOG_STYLE_MSGBOX, "BAN INFO", string, "Close", "");
	SetTimerEx("UnsetBan", 500, 0, "i", TargetID);
	PlayerInfo[TargetID][pBan] = 1;
	format(string, sizeof(string), "AdmCmd: %s has been banned by administrator %s. Reason: %s ",PlayerName(TargetID), PlayerName(playerid), reason);
	SendClientMessageToAll(COLOR_LIGHTRED, string);
	format(string, sizeof(string), "AdmCmd: %s has been banned by administrator %s. Reason: %s ", PlayerName(TargetID), PlayerName(playerid), reason);
	Log("logs/ban.log", string);
	return 1;
}

