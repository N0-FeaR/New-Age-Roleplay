CMD:changepass(playerid, params[])
{
    new string[256], Player[MAX_PLAYER_NAME];
    if(sscanf(params, "s[256]", params))return SendSyntaxMessage(playerid, "/changepass [password]");
    else
	{
	    GetPlayerName(playerid, Player, sizeof(Player));
		format(string, 256, "/Users/%s.ini", Player);
	    new INI:file = INI_Open(string);
	    INI_SetTag(file,"Account Info");
	    INI_WriteInt(file,"Password",udb_hash(params));
	    INI_Close(file);
	    SendServerMessage(playerid, "You change password to %s", params);
  	}
    return 1;
}
