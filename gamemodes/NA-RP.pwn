forward LoadBuyCarSys(playerid, type);
public LoadBuyCarSys(playerid,type)
{
    new string[128],loadcar,timetext[128],pricetext[128],qtext[100];
    PlayerPlaySound(playerid,1150, 0.0, 0.0, 0.0);
    switch(type)
    {
        case 0:
        {
            SetPVarInt(playerid, "NormalCarID", GetPVarInt(playerid, "NormalCarID")+1);
	        if(GetPVarInt(playerid, "NormalCarID") >= sizeof(NormalCar)) SetPVarInt(playerid, "NormalCarID", 0);
        }
        case 1:
        {
	        if(GetPVarInt(playerid, "NormalCarID") < 0) SetPVarInt(playerid, "NormalCarID", 78);
        }
    }
	DestroyVehicle(GetPVarInt(playerid, "NormalVeh"));
    loadcar = AddStaticVehicleEx(NormalCar[GetPVarInt(playerid, "NormalCarID")][carID],2226.3962,2457.6038,-7.4531,314.4251,GetPVarInt(playerid, "NormalColor1"),GetPVarInt(playerid, "NormalColor2"), 9999);
    SetVehicleVirtualWorld(loadcar,GetPlayerVirtualWorld(playerid));
    SetPVarInt(playerid, "NormalVeh", loadcar);
    if(GetPlayerMoneyEx(playerid) >= NormalCar[GetPVarInt(playerid, "NormalCarID")][carprice]) format(pricetext, sizeof(pricetext),"{66CC66}Price: $%d",NormalCar[GetPVarInt(playerid, "NormalCarID")][carprice]);
    else format(pricetext, sizeof(pricetext),"{FF3333}Price: $%d",NormalCar[GetPVarInt(playerid, "NormalCarID")][carprice]);
    if(PlayerInfo[playerid][pLevel] >= NormalCar[GetPVarInt(playerid, "NormalCarID")][carlevel]) format(timetext, sizeof(timetext),"{66CC66}Time-In-SA: %d",NormalCar[GetPVarInt(playerid, "NormalCarID")][carlevel]);
    else format(timetext, sizeof(timetext),"{FF3333}Time-In-SA: %d",NormalCar[GetPVarInt(playerid, "NormalCarID")][carlevel]);
    if(NormalCar[GetPVarInt(playerid, "NormalCarID")][carQ] >= 1) format(timetext, sizeof(timetext),"{66CC66}Quantity: %d (In-Stock)",NormalCar[GetPVarInt(playerid, "NormalCarID")][carQ]);
    else format(timetext, sizeof(timetext),"{FF3333}Quantity: %d (Out-Of-Stock)",NormalCar[GetPVarInt(playerid, "NormalCarID")][carQ]);
    format(string, sizeof(string),"%s\n%s\n%s\n%s", NormalCar[GetPVarInt(playerid, "NormalCarID")][carname],pricetext,timetext,qtext);
	return ShowPlayerDialog(playerid,DIALOG_DEALERSHIP2,DIALOG_STYLE_MSGBOX,"Dealership",string,"Next", "Options");
}
