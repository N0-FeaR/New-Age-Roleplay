public OnPlayerTakeDamage(playerid, issuerid, Float:amount, weaponid)
{
	if(IsPlayerConnected(issuerid))
	{
	    new Float:x, Float:y, Float:z, string[128], Float:amoun, Float:distance = 0.9, Float:health, Float:armour;
	    GetPlayerPos(playerid,x,y,z), GetPlayerHealth(playerid,health), GetPlayerArmour(playerid,armour);
	    if(weaponid == 23 && PlayerInfo[issuerid][pFaction] == 1 && GetPVarInt(issuerid, "LSPD_Ta") == 1 // Tazer System
		&& PlayerInfo[playerid][pDead] == 0 && PlayerInfo[issuerid][pCuffed] == 0
		&& GetPlayerState(playerid) == PLAYER_STATE_ONFOOT && PlayerInfo[playerid][pFaction] != 1 && GetPVarInt(playerid, "Member") != 3 && !IsPlayerInRangeOfPoint(issuerid,distance,x,y,z))
		{
		    if(GetPlayerWeapon(playerid) >= 22 && GetPlayerWeapon(playerid) <= 38) return SendClientMessage(issuerid,COLOR_GREY,"Cannot Taze someone with a weapon in hand."),SetPlayerHealth(playerid,health);
		    if(GetPVarInt(issuerid, "LSPD_Delay") > GetCount()) return SetPlayerArmedWeapon(playerid, 0), SendClientMessage(issuerid,COLOR_WHITE,"You must wait for your tazer to reload."), SetPlayerHealth(playerid,health);
		    if(!IsPlayerInRangeOfPoint(issuerid,35.0,x,y,z)) return SendClientMessage(issuerid,COLOR_GREY,"You are too far from the player your shooting at.");
			new sendername[MAX_PLAYER_NAME], giveplayer[MAX_PLAYER_NAME];
			format(sendername, sizeof(sendername), "%s", PlayerName(playerid));
	        format(giveplayer, sizeof(giveplayer), "%s", PlayerName(issuerid));
            format(string, sizeof(string), "*** %s aims %s tazer at %s and fires it.", giveplayer, CheckSex(issuerid), sendername);
            SendNearbyMessage(playerid, 30.0, COLOR_PURPLE, string);
   	        TogglePlayerControllable(playerid,false);
    	    SetPVarInt(playerid, "Cuffed", 1);
    	    SetPVarInt(playerid, "CuffedTime", 180);
    	    ApplyAnimation(playerid, "CRACK", "crckdeth2", 4.0, 1, 0, 0, 0, 0);
    	    PlayAudioStreamForPlayer(playerid, "http://www.pr-gta.com/pr-rp-soundeffects/shockpaddle-zap.mp3");
            SetPVarInt(issuerid, "LSPD_Delay",GetCount()+10000);
            SetPlayerArmour(playerid,armour);
			if(health < 10.0) SetPlayerHealth(playerid,10.0);
		    else SetPlayerHealth(playerid,health);
		    return true;
        }
        if(weaponid == 25 && GetPVarInt(issuerid, "BeanBag") == 1 && GetPlayerState(playerid) == PLAYER_STATE_ONFOOT // Bean-Bag System
		&& GetPVarInt(playerid, "Dead") == 0 && GetPVarInt(playerid, "Member") != 7 && health >= 10.0 && !IsPlayerInRangeOfPoint(issuerid,distance,x,y,z))
		{
		    if(!IsPlayerInRangeOfPoint(issuerid,35.0,x,y,z)) return SendClientMessage(issuerid,COLOR_GREY,"You are too far from the player your shooting at.");
		    format(string, sizeof(string),"You shot %s with a bean bag.", PlayerNameEx(playerid));
			SendClientMessage(issuerid,COLOR_WHITE,string);
			format(string, sizeof(string),"%s shot you with a bean bag.", PlayerNameEx(issuerid));
			SendClientMessage(playerid,COLOR_WHITE,string);
			TogglePlayerControllable(playerid,false);
    	    SetPVarInt(playerid, "Cuffed", 1);
    	    SetPVarInt(playerid, "CuffedTime", 60);
    	    ApplyAnimation(playerid, "PARACHUTE", "FALL_skyDive_DIE", 4.0, 0, 1, 1, 1, -1);
    	    SetPlayerArmour(playerid,armour);
    	    if(health < 10.0) SetPlayerHealth(playerid,10.0);
		    else SetPlayerHealth(playerid,health);
	        return true;
	    }
		if(weaponid == 8 || weaponid == 38)
		{
		    SetPlayerArmour(playerid,0.0);
		    SetPlayerHealth(playerid,0.0);
		    return true;
		}
		/*if(issuerid == INVALID_PLAYER_ID && !PlayerInWater(playerid)) // adding timer in the next update just want to flesh out a dialogue with /fdduty
        {
            if(GetPVarInt(playerid, "Member") == 4 && GetPVarInt(playerid, "FDDUTY") == 1)
			{
        		SetPlayerHealth(playerid, 99);
				return true;
			}
        }*/
		if(weaponid == 42)
		{
		    SetPlayerArmour(playerid,armour);
			if(health < 10.0) SetPlayerHealth(playerid,10.0);
		    else SetPlayerHealthEx(playerid,health+1.0);
		    ClearAnimations(playerid, true);
		    return true;
		}
	    if(weaponid >= 22 && weaponid <= 34 && amount >= 1.0 && armour <= 0 && health >= 10.0 && GetPVarInt(playerid, "Wound") == 0 && !IsPlayerInRangeOfPoint(issuerid,distance,x,y,z))
	    {
		    SetPVarInt(playerid, "Wound", 1);
		    SendClientMessage(playerid,COLOR_LIGHTRED,"WARNING: You currently have a gun wound you must recover at the hospital.");
		}
		if(weaponid >= 22 && weaponid <= 38)
	    {
		    SetPVarInt(issuerid, "HitMark", 1);
		    TextDrawHideForPlayer(issuerid, HitMark);
		    TextDrawShowForPlayer(issuerid, HitMark);
		}
		new damage = 0;
		switch(weaponid)
		{
		    case 22: damage = DAMAGE_COLT;
		    case 23: damage = DAMAGE_SILENCER;
		    case 24: damage = DAMAGE_DEAGLE;
		    case 25: damage = DAMAGE_SHOTGUN;
		    case 26: damage = DAMAGE_SAWNOFF;
		    case 28: damage = DAMAGE_UZI;
		    case 29: damage = DAMAGE_MP5;
		    case 30: damage = DAMAGE_AK47;
		    case 31: damage = DAMAGE_M4;
		    case 32: damage = DAMAGE_TEC9;
		    case 33: damage = DAMAGE_RIFLE;
		    case 34: damage = DAMAGE_SNIPER;
		}
		if(damage != 0 && !IsPlayerInRangeOfPoint(issuerid,distance,x,y,z))
		{
		    if(armour > 0)
		    {
		        amoun = armour - damage;
			    if(amoun < 0) return SetPlayerHealth(playerid,health-damage);
			    return SetPlayerArmour(playerid,armour-damage);
		    }
		    else return SetPlayerHealth(playerid,health-damage);
		}
	}
	return true;
}
