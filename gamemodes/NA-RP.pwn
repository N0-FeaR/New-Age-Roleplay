CMD:time(playerid, params[])
{
    new string[128],hour,minuite,second,year,month,day,mtext[50];
   	if(PlayerLogged[playerid] == false) SendErrorMessage(playerid, "You're not logged in.");
    if(PlayerInfo[playerid][pDead] > 0) return SendClientMessage(playerid, COLOR_WHITE, "Cannot use this command while dead.");
    if(PlayerInfo[playerid][pCuffed] > 0) return SendClientMessage(playerid, COLOR_GREY, "You can't do this while handcuffed.");
	gettime(hour,minuite,second);
	getdate(year, month, day);
	FixHour(hour);
	hour = shifthour;
	switch(month)
	{
		case 1: mtext = "January";
		case 2: mtext = "February";
		case 3: mtext = "March";
		case 4: mtext = "April";
		case 5: mtext = "May";
		case 6: mtext = "June";
		case 7: mtext = "July";
		case 8: mtext = "August";
		case 9: mtext = "September";
		case 10: mtext = "October";
		case 11: mtext = "November";
	    case 12: mtext = "December";
	}
	if(PlayerInfo[playerid][pJailed] > 0)
	{
	    format(string, sizeof(string), "JailTime Left: %d seconds.", PlayerInfo[playerid][pJailTime]);
	    return GameTextForPlayer(playerid, string, 3000, 1);
	}
	if(PlayerInfo[playerid][pWatch] == 0) return SendClientMessage(playerid,COLOR_GREY,"You don't have a watch.");
	if (minuite < 10)
	{
	    if (second < 10) format(string, sizeof(string), "~y~%s ~w~%d~n~~g~|~w~%d:0%d:0%d~g~|", mtext, day, hour, minuite, second);
	    else format(string, sizeof(string), "~y~%s ~w~%d~n~~g~|~w~%d:0%d:%d~g~|", mtext, day, hour, minuite, second);
	}
	else
	{
	    if (second < 10) format(string, sizeof(string), "~y~%s ~w~%d~n~~g~|~w~%d:%d:0%d~g~|", mtext, day, hour, minuite, second);
	    else format(string, sizeof(string), "~y~%s ~w~%d~n~~g~|~w~%d:%d:%d~g~|", mtext, day, hour, minuite, second);
	}
	GameTextForPlayer(playerid, string, 5000, 1);
	format(string, sizeof(string), "Playtime: %d seconds : Next Payday: %d minutes.", PlayerInfo[playerid][pPayDay], 60-PlayerInfo[playerid][pPayDay]);
	SendClientMessage(playerid, COLOR_LIGHTRED, string);
	ApplyAnimation(playerid,"COP_AMBIENT","Coplook_watch",4.1,0,0,0,0,0);
	return 1;
}
