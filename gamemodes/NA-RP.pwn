CMD:radio(playerid, params[])
{
	new text[128],string[256],sendername[MAX_PLAYER_NAME];
	if(sscanf(params, "s[128]", text)) SendClientMessage(playerid, COLOR_GREY, "Usage: /radio [text]");
	else
	{
	    if(PlayerLogged[playerid] == false) SendErrorMessage(playerid, "You're not logged in.");
	    if(PlayerInfo[playerid][pMute] == 1) return SendClientMessage(playerid, COLOR_LIGHTRED, "You are currently muted !");
	    if(PlayerInfo[playerid][pFaction] == 0) return SendClientMessage(playerid, COLOR_WHITE, "You must be in a faction to use this.");
	    if(PlayerInfo[playerid][pJailed] > 0) return SendClientMessage(playerid, COLOR_WHITE, "Cannot use this command while in-jail.");
	    if(PlayerInWater(playerid)) return SendClientMessage(playerid, COLOR_WHITE, "Cannot use this command while in-water !");
   		format(sendername, sizeof(sendername), "%s", PlayerName(playerid));
      	switch(PlayerInfo[playerid][pFaction])
      	{
      	    case 1:
      	    {
	  			if(PlayerInfo[playerid][pDuty] != 1) return SendErrorMessage(playerid, "You can not use the radio while not on duty.");
				else
				{
					new rank[50];
					switch(PlayerInfo[playerid][fRank])
					{
					    case 1: rank="Pvt.I";
					    case 2: rank="Pvt.FC";
					    case 3: rank="Pvt.SC";
					    case 4: rank="CPL";
					    case 5: rank="SGT";
					    case 6: rank="Staff.SGT";
					    case 7: rank="Master.SGT";
					    case 8: rank="SGT.Major";
					    case 9: rank="SGT.Major";
					    case 10: rank="SGT.Major";
					    case 11: rank="SGT.Major";
					    case 12: rank="SGT.Major";
					    case 13: rank="SGT.Major";
					    case 14: rank="SGT.Major";
					    case 15: rank="SGT.Major";
					    case 16: rank="SGT.Major";
					}
				    format(string, sizeof(string), "** %s_%s (radio): %s, over. **", rank, GetPlayerSurname(playerid,2), text);
				}
			}
      	    case 2 , 3:
      	    {
	  			//if(PlayerInfo[playerid][pdSuspensionDay] != 0) return SendClientMessage(playerid, COLOR_WHITE, "You are currently suspended.");
	  			if(PlayerInfo[playerid][pDuty] != 1) return SendClientMessage(playerid, COLOR_WHITE, "You can not use the radio while not on duty.");
				else{format(string, sizeof(string), "** %s (radio): %s, over. **", sendername, text);}
			}
			default: format(string, sizeof(string), "** %s (radio): %s. **", sendername, text);
      	}
      	if(PlayerInfo[playerid][pFaction] == 2)
      	{
		    if(GetPVarInt(playerid, "Duty") == 1 && GetPVarInt(playerid, "PDUNIT") != 0)
		    {
		        format(string, sizeof(string), "** %s[%s] (radio): %s, over. **", sendername, UnitNames[GetPVarInt(playerid, "PDUNIT")-1][0], text);
			    SendPoliceMessage(GetPVarInt(playerid, "PDRadio"),0x8080FF96,string);
			}
		    else SendPoliceMessage(GetPVarInt(playerid, "PDRadio"),0x8080FF96,string);
		}
        else SendFactionMessage(PlayerInfo[playerid][pFaction],0x8080FF96,string);
        ProxRadio(20.0, playerid, string, COLOR_FADE);
	}
	return 1;
}
