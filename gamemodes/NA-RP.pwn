forward OnPlayerFade(playerid, type);
public OnPlayerFade(playerid, type)
{
	switch(type)
	{
	    case 1: // From Fading In
	    {
	        switch(PlayerLogged[playerid])
	        {
	            case 1:
	            {
	                if(GetPVarInt(playerid,"FadeToDeath") == 1)
	                {
	                    DeletePVar(playerid, "FadeToDeath");
	                    PlayerInfo[playerid][pDead] = 3;
	                    PlayAudioStreamForPlayer(playerid, "http://rebound-rp.com/NA-RP-soundeffects/hospitalsound.mp3");
